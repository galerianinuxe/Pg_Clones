	// FORM DA PAGSEGURO
	$('#formPagamentoCartaoCreditoPagSeguro').validationEngine('validate');
	// FORM DO MERCADO PAGO
	$('#formPagamentoCartaoCreditoMercadoPago').validationEngine('validate');
});

