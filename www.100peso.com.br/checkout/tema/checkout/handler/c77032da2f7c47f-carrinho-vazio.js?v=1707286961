/**
* FUNÇÃO RESPONSÁVEL POR VALIDAR A RECUPERAÇÃO DE SENHA ESPECIFICA
*/
function solicitarRecuperacaoSenhaSimplificada(){

  var email          = $('.boxCamposRecuperacaoSimplificada .emailRecuperacaoSenhaSimplificada').val();
  var cpf            = $('.boxCamposRecuperacaoSimplificada .cpfRecuperacaoSenhaSimplificada').val();
  var dataNascimento = $('.boxCamposRecuperacaoSimplificada .dataRecuperacaoSenhaSimplificada').val();

  // PRELOADER
  var preloader = new Preloader;
  preloader.show('Estamos verificando seus dados');

  $.ajax({
    url: CAMINHO+'/checkout/ajax/ajax-recuperar-senha-simplificada.php',
    data: {email,cpf,dataNascimento},
    type: 'POST',
    dataType: 'JSON',
    success : function(retorno){
      if(retorno.status){
        swal({
          type: 'success',
          title: 'Sucesso',
          text: retorno.mensagem
        }, function(){
          window.location.href = retorno.url;
        });
      }else{
        $('.containerRecuperacaoSenhaSimplificada').html(retorno.html);
        $('.botaoRecuperacaoSenha').on('click',function(e){
          $('.containerRecuperacaoSenhaSimplificadaErro .alert').hide();
        });
      }
      preloader.hide();
    }
  });
}
// CLICK NO BOTAO RECUPERAR SENHA
$(document).on('click', '#recuperarSenhaSimplificada', function(){
  solicitarRecuperacaoSenhaSimplificada();
}); 

//FECHA OS ALERTAS
$(document).on('click','.alert .close',function(e){
	var alerta = $(this).closest('.alert');
	alerta.slideUp(300);
	alerta.css('opacity',0);
});
//FUNÇÃO QUE ATUALIZA OS SCRIPTS DOS INPUTS
function atualizarScriptsInput(){

  //ANIMAÇÃO LABEL
  $('[data-label]').each(function () {
    var label      = $(this).attr('data-label');
    var valorInput = $(this).val();

    if (valorInput == "" && $(this).prop('tagName') != 'SELECT') {
      let classes = 'mainBox-conteudo-form-input-label labelCadastro';
      if($(this).attr('disabled') !== undefined) classes += ' disabled';
      $(this).closest('.mainBox-conteudo-form-input').append('<div class="'+ classes +'">'+label+'</div>');
    }
    else {
      $(this).closest('.mainBox-conteudo-form-input').append('<div class="mainBox-conteudo-form-input-label labelCadastro inpAtivo">'+label+'</div>');
    }
  });
  $('[data-label]').focus(function () {
    var label = $(this).closest('.mainBox-conteudo-form-input').find('.mainBox-conteudo-form-input-label');
    label.addClass('inpAtivo').addClass('colorir');

    $(this).focusout(function () {
      var valorInput = $(this).val();
      label.removeClass('colorir');
      if (valorInput == "" && $(this).prop('tagName') != 'SELECT') {
        label.removeClass('inpAtivo');
      }
    });
  });
  $('.mainBox-conteudo-form-input-label').click(function () {
    $(this).closest('.mainBox-conteudo-form-input').find('input').focus();
  });



  //VALIDAÇÃO VALIDATE
  $('[class*="validate["]').not('.inputSemEstiloValidacao').each(function () {
    $(this).wrap('<div class="mainBox-conteudo-form-input-validacao">');
  });
  $('.mainBox-conteudo-form-input-validacao select').each(function () {
    $(this).closest('.mainBox-conteudo-form-input-validacao').addClass('mainBox-conteudo-form-input-validacao-select');
  });
  $('.boxRecaptchaCadastro .mainBox-conteudo-form-input-validacao, .boxCheckout-reCaptchaPagamento .mainBox-conteudo-form-input-validacao, .boxRecaptchaLogin .mainBox-conteudo-form-input-validacao').each(function () {
    $(this).addClass('mainBox-conteudo-form-input-validacao-recaptcha');
  });


  //AJUSTAR ICONE NO CAMPO DE SENHA
  $('.mainBox-conteudo-form-input .mostraOcultaSenha:visible').closest('.mainBox-conteudo-form-input').find('.mainBox-conteudo-form-input-validacao').addClass('iconeAjustado');
}

//ATUALIZA OS SCRIPTS DOS INPUTS
atualizarScriptsInput();

//BOTÃO DE VISUALIZAR SENHA
$(document).on('click','.formularioCheckout .botaoVisualizarSenha',function(){
  //ATIVA/DESATIVA OS ICONES DE VISZUALIZAÇÃO DE SENHA
  $(this).find('.mostra').toggle();
  $(this).find('.oculta').toggle();
  //VERIFICA TIPO DO CAMPO
  var campoSenha = $(this).closest('.formularioCheckout').find('input[name="senha"]');
  if (campoSenha.attr('type') == "text") {
    campoSenha.attr('type', 'password');
  }else{
    campoSenha.attr('type', 'text');
  }
});

/**
 * Ação responsável por validar se o valor digitado é numérico
 */
$('.validacaoNumerica').on('keypress', function(event){
  //TRATAMENTO DE VALORES NUMÉRICOS
  (new FiltroNumerico).validateNumericKeyPress(event);
});/**
 * Classe responsável pelos tratamentos de valores numéricos
 * 
 * @author David Guimarães
 */
class FiltroNumerico{

  /**
   * Método responsável por retornar apenas os valores numéricos de um texto
   * @param  {string} valor Texto a ser filtrado
   * @return {int}          Retorna os valores numéricos
   */
  getNumeros(valor){
    const expressaoRegular = /[^\d]+/g;
    return valor.replace(expressaoRegular, '');
  }

  /**
   * Método responsável por verificar se o texto é numérico
   * @param {string} valor 
   * @return {bool}
   */
  isNumeric(valor){
    const expressaoRegular = /^[0-9.\/-]+$/;
    return expressaoRegular.test(valor);
  }

  /**
   * Método responsável por verificar se o dado de entrada é estritamente numéricos
   * @param  {string} valor 
   * @return {bool}
   */
  isOnlyNumbers(valor){
    const expressaoRegularNumerica = /[0-9]+/;
    return expressaoRegularNumerica.test(valor);
  }

  /**
   * Método responsável por validar o evento keypress para validação do dados de entrada da tecla pressionada
   */
  validateNumericKeyPress(event){
    if(!this.isOnlyNumbers(event.key)) event.preventDefault();
  }
}// MASCARA DE CPF COM UM CAMPO A MAIS PARA PODER PASSAR NA VALIDACAO E TROCAR A MASCARA PARA CNPJ
$('.mascaraCPFAdaptada').maskcheckout('000.000.000-000');
// ADICIONA A CLASSE DE AJUSTE SOMENTE NO CAMPO DE EMAIL
$(document).on('focus', '.intermediadorEmailCliente',function(){
  $(this).addClass('ajustaMascarasCpfCnpjEmail');
});

$(document).on('keyup', '.ajustaMascarasCpfCnpjEmail', function () {
	var caracter, element;
	element = $(this);
	caracter = element.val();

  //OBJETO DA CLASSE DE TRATAMENTO DE VALORES NUMÉRICOS
  const obFiltroNumerico = new FiltroNumerico;

	$(this).parent().find('.formError').remove();
  // CAMPO VAZIO
  if(caracter.length === 0){
    element.unmaskcheckout();
    element.removeClass();
    element.addClass('ajustaMascarasCpfCnpjEmail validate[required]');
    return false;
  }
  // EMAIL
  if(!obFiltroNumerico.isNumeric(caracter)){
    element.removeClass();
    element.addClass('ajustaMascarasCpfCnpjEmail validate[required,custom[email]]');
  }else{
    var tipoLoginPermitido = $('.ajustaMascarasCpfCnpjEmail').data('tipo-login-permitido');

    //FILTRA APENAS OS NÚMEROS
    let cpfCnpj = obFiltroNumerico.getNumeros(caracter);

    switch (tipoLoginPermitido) {
      case 'cpf':
        element.removeClass();
        element.addClass('ajustaMascarasCpfCnpjEmail validate[required,custom[cpf]] mascaraCPF');
        break;
      case 'cnpj':
        element.removeClass();
        element.addClass('ajustaMascarasCpfCnpjEmail validate[required,custom[cnpj]] mascaraCNPJ');
        break;
      default: 
        // CPF
        element.removeClass();
        element.addClass('ajustaMascarasCpfCnpjEmail validate[required,custom[cpf]] mascaraCPFAdaptada');
        // CNPJ
        if (cpfCnpj.length >= 14) {
          element.removeClass();
          element.addClass('ajustaMascarasCpfCnpjEmail validate[required,custom[cnpj]] mascaraCNPJ');
        }
        break;
    }
  }
  element.removeAttr('maxlength');
}).trigger('keyup');$(document).ready(function(e){
  $('.tabelaAzul-inputRadio').change(function(e){
    $('tr.popupFretePickup-containerPopup-itens').hide();
    if($(this).hasClass('inputPickup')){
      var id = $(this).val();
      $('tr[data-pickup='+id+']').show();
    }
  });
});
//VERIFICA POST
if($('.topoCheckoutCadastro').html() && $('.topoCheckoutCadastro').data('status-post')){

  //REMOVE O REENVIO DO POST ATUALIZANDO O HISTÓRICO DAS URLS
  var url = window.location.href;
  var titulo = $('title').html();
  const stateObj = { pagina: "Remoção de POST" };
  history.pushState(stateObj,titulo, url);
  
}
/**
 * Classe responsável pelo preloader
 * @method Preloader
 */

function Preloader(seletor){

  /**
   *  SELETOR PARA APLICAR O PRELOADER EM UMA DIV ESPECIFICA 
   */
  this.seletor = seletor;


  /**
   * MOSTRA O PRELOADER FULL
   */
  this.show = function(mensagem){
    var html = '<div class="preloaderFull flex-container align-middle align-center"><div><span class="preloaderFull-texto">'+mensagem+'<span class="preloaderFull-pontos"></span></span><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid" class="lds-rolling" style="background: none;"><defs><style>.preloaderCheckout{stroke: currentColor}</style></defs><circle class="preloaderCheckout" cx="50" cy="50" fill="none" ng-attr-stroke="{{config.color}}" ng-attr-stroke-width="{{config.width}}" ng-attr-r="{{config.radius}}" ng-attr-stroke-dasharray="{{config.dasharray}}" stroke-width="13" r="30" stroke-dasharray="141.37166941154067 49.12388980384689" transform="rotate(329.958 50 50)"><animateTransform attributeName="transform" type="rotate" calcMode="linear" values="0 50 50;360 50 50" keyTimes="0;1" dur="1s" begin="0s" repeatCount="indefinite"></animateTransform></circle></svg></div></div>';
    $('body').append(html);
    $('.preloaderFull').hide();
    $('.preloaderFull').fadeIn(0);
  }


  /**
   * ESCONDE O PRELOADER FULL 
   */
  this.hide = function(){
    $('.preloaderFull').fadeOut(0,function(e){
      $('.preloaderFull').remove();
    });
  }


  /**
   * MOSTRA O PRELOADER EM UMA DIV 
   */
  this.showDiv = function(){
    var html = '<div class="preloaderDiv"><div class="preloaderDivImagem"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid" class="lds-rolling" style="background: none;"><defs><style>.preloaderCheckout{stroke: currentColor}</style></defs><circle class="preloaderCheckout" cx="50" cy="50" fill="none" ng-attr-stroke="{{config.color}}" ng-attr-stroke-width="{{config.width}}" ng-attr-r="{{config.radius}}" ng-attr-stroke-dasharray="{{config.dasharray}}" stroke-width="13" r="30" stroke-dasharray="141.37166941154067 49.12388980384689" transform="rotate(329.958 50 50)"><animateTransform attributeName="transform" type="rotate" calcMode="linear" values="0 50 50;360 50 50" keyTimes="0;1" dur="1s" begin="0s" repeatCount="indefinite"></animateTransform></circle></svg></div></div>';
    $(this.seletor).append(html);
    $(this.seletor).data('position',$(this.seletor).css('position'));
    $(this.seletor).css('position','relative');
  }
  

  /**
   * ESCONDE O PRELOADER QUE ESTA APLICADO A UMA DIV
   */
  this.hideDiv = function(){
    $(this.seletor).css('position',$(this.seletor).data('position'));
    $('.preloaderDiv').remove();
  }

}//CANCELAR VENDA INTERNA
$(document).on('click','.cancelarVendaInternaLink',function(){
  //JANELA DE CONFIRMACAO
  var confirmCustomVars = {
    title: "Aviso",
    text: "Clique em OK para confirmar o cancelamento da venda.",
    type: "warning",
    showCancelButton: true,
    confirmButtonColor: "#24a64c",
    confirmButtonText: "OK",
    cancelButtonText: "Cancelar",
    closeOnConfirm: true,
    html:true
  };
  retorno = function(e){
    if(e){
      //AJAX QUE CANCELA A VENDA INTERNA
      jQuery.ajax({
        type: "POST",
        url: CAMINHO_WAPSTORE+"/ajax/modulo/venda-interna/ajax-modulo-venda-interna-cancelar-venda.php",
        dataType : "json",
        success: function(data){
          if(data.status){
            //REDIRECIONA PARA O PAINEL WAPSTORE
            window.location = window.location.href;
          }else{
            //ERRO AO CANCELAR A VENDA
            notify('Erro ao cancelar a venda interna.', 'error');
          }
        }
      });
    }
  }
  confirmCustom(confirmCustomVars,retorno);
});

function addEventChangeOptionCliente() {
  $('.input-opcao-cliente-venda-interna').on('change', () => {
    $('.mainBox-conteudo-form-bottom-btn button').removeClass('displayNone');
  });
}

$('.btnPesquisarCliente').on('click', (e) => {
  e.preventDefault();

  buscarClienteByBusca();
});

$('.termoBuscaCliente').on('keydown', (e) => {
  if(e.key == 'Enter') {
    e.preventDefault();
    buscarClienteByBusca();
  } 
});

function buscarClienteByBusca() {
  let termoBusca = $('.termoBuscaCliente').val();

  //AJAX QUE CANCELA A VENDA INTERNA
  jQuery.ajax({
    type: "POST",
    url: CAMINHO+"/checkout/ajax/venda-interna/ajax-checkout-venda-interna-buscar-cliente.php",
    dataType : "json",
    data: {termoBusca},
    success: function(data){
      if(data.sucesso){
        $('.box-busca-clientes-results').html(data.html);
        addEventClickLabelCliente();
        addEventChangeOptionCliente();
      }
    }
  });
}

//FUNÇÃO QUE ATUALIZA O TOPO DA VENDA INTERNA
function atualizarTopoVendaInterna(){
  if(typeof VENDA_INTERNA === "undefined" || !VENDA_INTERNA) return false;
  //AJAX QUE ATUALIZA O TOPO DA VENDA INTERNA
  jQuery.ajax({
    type: "POST",
    url: CAMINHO+"/ajax/estrutura/venda-interna/ajax-estrutura-venda-interna-atualizar-topo.php",
    dataType : "json",
    success: function(data){
      if(data.status){
        var topo = $('.topoVendaInterna');
        //QUANTIDADE DE ITENS
        topo.find('.itensCarrinho').html(' '+data.itensCarrinho);
        //VALOR TOTAL
        topo.find('.totalCarrinho').html(' '+data.totalCarrinho);
      }
    }
  });
}

$(document).on('submit','#formCadastroClienteVendaInterna',function(e){
  e.preventDefault();

  //VERIFICA MENSAGENS DE EMAIL OU CPF/CNPJ JÁ CADASTRADO
  var boxMensagem = $(this).find('.boxInputMensagemCheckoutCadastro.mensagemAtiva, .valida-input.invalido');
  if(boxMensagem.html()){

    //ANCORA MENSAGEM
    var targetOffset = boxMensagem.offset().top;
    $('html, body').animate({
      scrollTop: targetOffset - 100
    }, 500);

    //FOCUS NO INPUT
    boxMensagem.find('input').first().focus();

    return false;
  }

  cadastrarClienteVendaInterna();
});

$(document).on('focusout', '#cpf-cadastro', function() {
	let cnpj = $(this).val().replace(/[^0-9]/g, '');
	
	if(cnpj.length == 14) {
		$.ajax({
			type: "POST",
			url: CAMINHO + '/checkout/ajax/endereco/intermediacao/ajax-checkout-endereco-intermediacao-buscar-cliente-receita.php',
			data: { cnpj },
			dataType: "json",
			success: (data) => {
				if(data.status && data.cliente !== undefined) {
					// DEFINE OS INPUTS
					let inputEmail        = $('[name = "email"]');
					let inputNomeRazao    = $('[name = "nomeRazao"]');
					let inputNomeFantasia = $('[name = "nomeFantasia"]');
					let inputCelular      = $('[name = "celular"]');
					let inputTelefone     = $('[name = "telefone"]');

					// SETA OS DADOS RETORNADOS
					inputEmail.val(data.cliente.email);
					inputNomeRazao.val(data.cliente.nomeRazao);
					inputNomeFantasia.val(data.cliente.nomeFantasia);
					inputCelular.val(data.cliente.celular);
					inputTelefone.val(data.cliente.telefone);

					// BLOQUEIO DE CAMPOS PELA RECEITA
					let alterar   = data.alterarCampos;
					let nomeRazao = $('#nome-cadastro');
					let telefone  = $('#tel-fixo-cadastro');
					alterar ? nomeRazao.removeAttr('readonly'): nomeRazao.attr('readonly', true);
					alterar ? telefone.removeAttr('readonly'): telefone.attr('readonly', true);

					// DEIXA OS INPUTS ATIVOS
					inputNomeRazao.parent().parent().find('.labelCadastro').addClass('inpAtivo');
					inputTelefone.parent().find('.labelCadastro').addClass('inpAtivo');

					// MOSTRA A OPÇÃO DE ISENÇÃO
					$('.boxOpcaoIsento').parent().show();
				}
			},
			error: (e) => {
				console.log(e);
			}
		});
	}
});


// VALIDACOES PARA O EMAIL A SER CADASTRADO, VERIFICA SE JÁ A CLIENTE COM O EMAIL
$(document).on('change', '#formCadastroClienteVendaInterna input[name="email"]', function (e) {
	validarEmailCadastro(this);
	return false;
});

$(document).on('keyup', '#formCadastroClienteVendaInterna input[name="email"]', function (e) {
	//TECLA PRESSIONADA
	var keyCode =  e.keyCode  ? e.keyCode  :
                 e.charCode ? e.charCode :
                 e.which    ? e.which    : void 0;

	//VERIFICA SE NÃO É O ENTER
  if(keyCode != 13){
		clearTimeout(timeoutValidarEmailCadastro);
		var elemento = this;
		timeoutValidarEmailCadastro = setTimeout(function(e){validarEmailCadastro(elemento)},1000);
	}else {
		validarEmailCadastro(elemento);
	}
});


// VALIDACOES DE CPF/CNPJ A SER CADASTRADO, VERIFICA SE JÁ HÁ CLIENTE COM O CPF/CNPJ
$(document).on('change', '#formCadastroClienteVendaInterna input[name="cpfCnpj"]', function (e) {
	validarCpfCnpjCadastro(this);
	return false;
});

$(document).on('keyup', '#formCadastroClienteVendaInterna input[name="cpfCnpj"]', function (e) {
	//TECLA PRESSIONADA
	var keyCode =  e.keyCode  ? e.keyCode  :
                 e.charCode ? e.charCode :
                 e.which    ? e.which    : void 0;

	//VERIFICA SE NÃO É O ENTER
  if(keyCode != 13){
		clearTimeout(timeoutValidarCpfCnpjCadastro);
		var elemento = this;
		timeoutValidarEmailCadastro = setTimeout(function(e){validarCpfCnpjCadastro(elemento)},1000);
	}else {
		validarCpfCnpjCadastro(elemento);
	}
});

/**
 * Função responsável por setar os dados do cliente no formulário estático
 * @param {object} dadosCliente 
 */
function setarDadosEstaticosCliente(dadosCliente) {
  $('.dados-cliente-setado-nome').text(dadosCliente.nomeRazao);
  $('.dados-cliente-setado-email').text(dadosCliente.email);
  $('.dados-cliente-setado-cpfCnpj').text(dadosCliente.cpfCnpj);

  $('.box-cliente-setado').removeClass('displayNone');
  $('#formBuscaClientesVendaInterna').addClass('displayNone');
  $('.box-cadastro-cliente-venda-interna').addClass('displayNone');
  $('.mainBox-conteudo-form-bottom-btn button').removeClass('displayNone');
  $('[name="idCliente"]').val(dadosCliente.id);
  $('.box-busca-clientes-results').html('');
  $('.box-cadastro-cliente').slideUp();
  $('.btn-cadastro-cliente-toggle i').removeClass('arrow-up');
}

/**
 * Função responsável por exibir o formulário de busca de clientes para a venda interna
 * @returns {void}
 */
function exibirBuscaCliente() {
  $('.box-cliente-setado').addClass('displayNone');
  $('#formBuscaClientesVendaInterna').removeClass('displayNone');
  $('.box-cadastro-cliente-venda-interna').removeClass('displayNone');
  $('.mainBox-conteudo-form-bottom-btn button').addClass('displayNone');
  $('.box-busca-clientes-results').html('');
  $('[name="idCliente"]').val(0);
}

/**
 * Função resposnável por setar cliente existente
 * 
 * @param   {string} campo     Valor do campo
 * @param   {string} tipoCampo Tipo do campo
 * @returns {void}
 */
function setarClienteExistente(campo) {
  var url = CAMINHO + '/checkout/ajax/venda-interna/ajax-checkout-venda-interna-consultar-cliente.php';

  //INICIA PRELOADER
  var preloader = new Preloader();
  preloader.show('Associando cliente...');

  $.ajax({
    type: "POST",
    url: url,
    data: {
      campo: campo
    },
    dataType: "json",
    success: function success(data) {
      preloader.hide();

      if(!data.retorno) return;

      setarDadosEstaticosCliente(data.retorno);
    },
    error: function error(e) {
      //REMOVE PRELOADER
      preloader.hide();
      console.log(e);
    }
  });
}

/**
* Função para setar o cliente final da compra
* @param  string campo (email ou cpf/cnpj do cliente)
* @return layout
*/
function cadastrarClienteVendaInterna() {
  var url      = CAMINHO + '/checkout/ajax/venda-interna/ajax-checkout-venda-interna-cadastrar-cliente.php';
  var myForm   = document.getElementById('formCadastroClienteVendaInterna');
  var formData = new FormData(myForm);
  
  //INICIA PRELOADER
  var preloader = new Preloader();
  preloader.show('Cadastrando cliente...');

  $.ajax({
    type: "POST",
    url: url,
    data: formData,
    dataType: "json",
    cache: false,
    processData: false,
    contentType: false,
    success: function success(data) {
      preloader.hide();

      //VERIFICA MENSAGEM DE ERRO
      if(data.status === false && data.mensagem !== null){
        $('.boxFormularioMensagemAlerta').html(data.mensagem);
        return;
      }

      setarDadosEstaticosCliente(data.cliente);
    },
    error: function error(e) {
      //REMOVE PRELOADER
      preloader.hide();
      console.log(e);
    }
  });
}

//INICIALIZA A VENDA INTERNA
$(document).on('click', '.mainBox-conteudo-form-bottom-btn button', () => {
  var url = CAMINHO + '/checkout/ajax/venda-interna/ajax-checkout-venda-interna-iniciar.php';

  var dadosVendaInterna = {
    idCliente: $('[name="idCliente"]').val(),
    configuracaoPagamento: $('[name="configuracaoPagamento"]').val(),
    emailTransacional: $('[name="emailTransacional"]').val()
  };
  
  //INICIA PRELOADER
  var preloader = new Preloader();
  preloader.show('Iniciando Venda Interna...');

  $.ajax({
    type: "POST",
    url: url,
    data: dadosVendaInterna,
    dataType: "json",
    success: function success(data) {
      preloader.hide();

      if(data.status) location.reload();
    },
    error: function error(e) {
      //REMOVE PRELOADER
      preloader.hide();
      console.log(e);
    }
  });
});

// SETAR CLIENTE A PARTIR DO CADASTRO
$(document).on('click', '.setarClienteCadastrado', function(){
	var campo = $(this).attr('data-campo');

	setarClienteExistente($('input[name="'+campo+'"]').val());
});


$('.btn-cadastro-cliente').on('click', () => {
  $('.box-cadastro-cliente').slideToggle();
  $('.btn-cadastro-cliente-toggle i').toggleClass('arrow-up');
});

$('.alterar-cliente-venda-interna').on('click', () => {
  exibirBuscaCliente()
});

function addEventChangeOptionCliente() {
  $('.input-opcao-cliente-venda-interna').on('change', () => {
    $('.mainBox-conteudo-form-bottom-btn button').removeClass('displayNone');
  });
}

function addEventClickLabelCliente() {
  $('.dados-cliente').on('click', (e) => {
    $(e.currentTarget).parent().find('.label-opcao-cliente').trigger('click');

    $('[name="idCliente"]').val($(e.currentTarget).closest('.opcaoCliente').data('id-cliente'));
  });

  $('.label-opcao-cliente').on('click', (e) => {
    $('[name="idCliente"]').val($(e.currentTarget).closest('.opcaoCliente').data('id-cliente'));
  });
}

$('.more-details-topo-icon').on('click', () => {
  $('.more-details-content').toggle(100);
});

addEventClickLabelCliente();
addEventChangeOptionCliente();
/**
 * Função para alterar o cliente final da compra caso esteja setado
 */
function alterarCliente() {
	var url = CAMINHO + '/checkout/ajax/endereco/intermediacao/ajax-checkout-endereco-intermediacao-alterar-cliente.php';

	//INICIA PRELOADER
	var preloader = new Preloader();
	preloader.show('Removendo cliente atual...');

	$.ajax({
		type: "POST",
		url: url,
		dataType: "json",
		success: function success(data) {
			// CASO ESTEJA EM PAGAMENTO, VOLTA PRO CHECKOUT ENDEREÇO
      if(data.status === true) window.location.href = CAMINHO+'/checkout/endereco';

			preloader.hide();
		},
		error: function error(e) {
			//REMOVE PRELOADER
			preloader.hide();
			console.log(e);
		}
	});
}

// ALTERAR CLIENTE
$(document).on('click', '.alterarClienteFinal', function(){
  alterarCliente();
});

/**
 * Ação responsável por efetuar o submit do formulário da popup de cadastro/atualização de uma filial
 */
$(document).on('submit', '#formCadastroMatrizFilial', function(e) {
  e.preventDefault();

  const preloader = new Preloader;
  preloader.show('Processando...');

  const formData = new FormData(e.currentTarget);
	formData.append('identificadorFilial', $(this).find('[data-filial]').attr('data-filial'));
  formData.append('rewrite', $('.cadastrarFilial', window.parent.document).data('filial-rewrite'));

  $.ajax({
    url: CAMINHO+'/checkout/ajax/registro-filiais/ajax-checkout-registro-filiais.php',
    type: 'POST',
    data: formData,
    dataType: 'json',
    cache: false,
		contentType: false,
		processData: false,
    success: function(data) {
      preloader.hide();

			if(!data.sucesso) {
        var alertCustomVars = {
          title: "Erro",
          text: data.mensagem,
          type:"error",
          confirmButtonText: "Fechar",
          allowEscapeKey:false,
          html:true
        };
        alertCustom(alertCustomVars);
        return;
      }

      var alertCustomVars = {
        title: "Sucesso",
        text: data.mensagem,
        type:"success",
        confirmButtonText: "Fechar",
        allowEscapeKey:false,
        html:true
      };

      alertCustom(alertCustomVars, function(e) {
        if(e) parent.location.reload();
      });
    },
    error: function(data) {
      preloader.hide();
    }
  });
});// SETAR TIPO DE VENDA PELO SELECT
$(document).on('blur', '.tipoVendaSelect', function () {
  var idTipoVenda = $(this).find(':selected').val();
  setarTipoVenda(idTipoVenda);
});

/**
 * Ação responsável por remover um tipo de venda da sessão e redirecionar o cliente para a alteração de tipo de venda.
 */
$(document).on('click', '#alterarTipoVenda', function () {
  let url       = CAMINHO + '/checkout/ajax/endereco/tipo-venda/ajax-checkout-endereco-remover-tipo-venda.php';
  let preloader = new Preloader;
  preloader.show('Removendo tipo de venda atual...');

  $.ajax({
    type: 'POST',
    url: url,
    dataType: 'json'
  }).done(() => window.location = CAMINHO + '/checkout/endereco');
});

/**
 * Função para setar o tipo de venda na sessão
 * @param {int} idTipoVenda
 */
function setarTipoVenda(idTipoVenda) {
  var idTipoVenda = (idTipoVenda !== undefined) ? idTipoVenda : null;
  var url         = CAMINHO + '/checkout/ajax/endereco/tipo-venda/ajax-checkout-endereco-atualizar-tipo-venda.php';
  let preloader   = new Preloader;
  preloader.show('Aplicando tipo de venda...');

  $.ajax({
    type: "POST",
    url: url,
    data: { 'idTipoVenda': idTipoVenda },
    dataType: "json",
    success: function success() {
      window.location = CAMINHO + '/checkout/endereco';
    },
    error: function error(e) {
      console.log(e);
      preloader.hide();
    }
  });
}// ATUALIZA OS VALORES AO ALTERAR O ATRIBUTO SIMPLES
function atualizarAtributoSimplesBrinde(obj){
  boxUpsellBrinde  = $(obj).closest('.dadosUpsellBrinde');
  idProduto        = $(obj).attr('data-id-produto');
  idAtributo       = $(obj).val();
  idBrinde         = boxUpsellBrinde.attr('data-id-brinde');

  var preloader = new Preloader();
	preloader.show('Atualizando brinde...');

  $.ajax({
    type:'POST',
    url: CAMINHO+'/checkout/ajax/upsell/brinde/ajax-upsell-brinde-produto-atributo-simples.php',
    data:{
      idProduto:idProduto,
      idAttrValor:idAtributo,
      idBrinde:idBrinde,
    },
    dataType : 'json',
    success:function(data) {
      boxUpsellBrinde.find('.upsell-produto-img img').attr('src',data.imagem);
      
      preloader.hide();
    },
    error:function(data){
      preloader.hide();
    }
  });
}

// ATUALIZA O PRODUTO AO ALTERAR O ATRIBUTO UNICO
function atualizarAtributoUnicoBrinde(obj){
  boxUpsellBrinde = $(obj).closest('.upsellBoxBrinde');
  idProduto       = $(obj).val();
  idUpsell        = $(obj).closest('.upsellBox').attr('data-id-upsell');

  var preloader = new Preloader();
  preloader.show('Atualizando brinde...');

  $.ajax({
    type:'POST',
    url: CAMINHO+'/checkout/ajax/upsell/brinde/ajax-upsell-brinde-produto-atributo-unico.php',
    data:{
      idProduto:idProduto,
      idUpsell:idUpsell,
    },
    dataType : 'json',
    success:function(data) {
      boxUpsellBrinde.html(data.layout);
      preloader.hide();
    },
    error:function(data){
      preloader.hide();
    }
  });
}

///////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////


$(document).on('change','.upsellBoxBrinde select[name="upsellAtributoSimples"]', function(){
  atualizarAtributoSimplesBrinde($(this));
});

$(document).on('change','.upsellBoxBrinde select[name="upsellAtributoUnico"]', function(){
  atualizarAtributoUnicoBrinde($(this));
});
// ATUALIZA OS VALORES AO ALTERAR O ATRIBUTO SIMPLES
function atualizarAtributoSimples(obj){
  boxUpsell     = $(obj).closest('.upsellBox');
  idProduto     = $(obj).attr('data-id-produto');
  idAtributo    = $(obj).val();
  idUpsell      = boxUpsell.attr('data-id-upsell');

  var preloader = new Preloader();
	preloader.show('Atualizando oferta...');

  $.ajax({
    type:'POST',
    url: CAMINHO+'/checkout/ajax/upsell/ajax-upsell-produto-atributo-simples.php',
    data:{
      idProduto:idProduto,
      idAttrValor:idAtributo,
      idUpsell:idUpsell,
    },
    dataType : 'json',
    success:function(data) {
      boxUpsell.find('.precoPorUpsell').text(data.precoPor);
      boxUpsell.find('.precoUpsell').text(data.precoUpsell);
      boxUpsell.find('.descontoUpsell').text(data.descontoUpsell);
      boxUpsell.find('.upsellBoxProduto .upsell-produto-img img').attr('src',data.imagem);
      
      preloader.hide();
    },
    error:function(data){
      preloader.hide();
    }
  });
}

// ATUALIZA O PRODUTO AO ALTERAR O ATRIBUTO UNICO
function atualizarAtributoUnico(obj){
  boxUpsellProd = $(obj).closest('.upsellBoxProduto');
  idProduto     = $(obj).val();
  idUpsell      = $(obj).closest('.upsellBox').attr('data-id-upsell');

  var preloader = new Preloader();
  preloader.show('Atualizando oferta...');

  $.ajax({
    type:'POST',
    url: CAMINHO+'/checkout/ajax/upsell/ajax-upsell-produto-atributo-unico.php',
    data:{
      idProduto:idProduto,
      idUpsell:idUpsell,
    },
    dataType : 'json',
    success:function(data) {
      boxUpsellProd.html(data.layout);
      preloader.hide();
    },
    error:function(data){
      preloader.hide();
    }
  });
}

///////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////


$(document).on('change','.upsellBoxProduto select[name="upsellAtributoSimples"]', function(){
  atualizarAtributoSimples($(this));
});

$(document).on('change','.upsellBoxProduto select[name="upsellAtributoUnico"]', function(){
  atualizarAtributoUnico($(this));
});
function carregarSliderUpsell(){
  $('.slider-upsell').not('.slick-initialized').slick({
    slidesToShow: 3,
    arrows: true,
    infinite: true,
    dots: false,
    responsive: [
      {
        breakpoint: 1024,
        settings: {
          slidesToShow: 2,
          dots: false,
        }
      },
      {
        breakpoint: 768,
        settings: {
          slidesToShow: 1,
          dots: false,
        }
      }
    ]
  });  
}

function carregarSliderUpsellPagamento(box){
  box.find('.slider-upsell-box-pagamento').not('.slick-initialized').slick({
    slidesToShow: 1,
    arrows: true,
    infinite: true,
    dots: false
  });
}
/**
* Responsável por selecionar o upsell, adicionando ao carrinho
*/
function adicionarUpsell(id, boxUpsell){
  let item = {
    id: boxUpsell.find('.dadosProdutoUpsell').attr('data-id-item'),
    localOferta: boxUpsell.attr('data-local-oferta'),
    quantidade: boxUpsell.find('.upsellQuantidade input').val(),
    idAtributoValor: boxUpsell.find('.dadosProdutoUpsell select[name="upsellAtributoSimples"]').val(),
  };

  let brinde = {};
  if(boxUpsell.find('.upsellBoxBrinde')){
    brinde = {
      id: boxUpsell.find('.dadosUpsellBrinde').attr('data-id-produto'),
      idAtributoValor: boxUpsell.find('.dadosUpsellBrinde select[name="upsellAtributoSimples"]').val(),
    };
  }

  var preloader = new Preloader;
  preloader.show('Adicionando oferta...');

  $.ajax({
    url: CAMINHO+'/checkout/ajax/upsell/ajax-upsell-adicionar.php',
    data: {id, item, brinde},
    type: 'POST',
		dataType:"json",
    success: function(data){
      if(data.sucesso){
        location.reload();
        return;
      }
      
      if(typeof data.quantidadeLimite != 'undefined'){
        boxUpsell.find('.upsellQuantidade input').val(data.quantidadeLimite)
        emitirAlerta(boxUpsell.find('.upsellQuantidade input'));
      } 
      
      preloader.hide();
    }
  });
}

/**
* Responsável por remover um upsell do carrinho, em qualquer momento do checkout
*/
function removerUpsell(hash,tipo){
  var preloader = new Preloader;
  preloader.show('Removendo oferta...');
  $.ajax({
    url: CAMINHO+'/checkout/ajax/upsell/ajax-upsell-remover.php',
    data: {hash,tipo},
    type: 'POST',
		dataType:"json",
    success: function(data){
      if(data.sucesso){
        location.reload();
        return;        
      }

      preloader.hide();
    }
  });
}

/**
* Responsável por remover o slide de ofertas em qualquer momento do checkout por um dia
*/
function esconderUpsells(){
  // SETA O COOKIE PARA ESCONDER POR UM DIA
  let exp = new Date(Date.now() + 86400e3);
  exp = exp.toUTCString();
  document.cookie = 'esconderUpsell=true; expires='+exp+';path=/';
  
  $('.upsell-sec').hide();
  $('.upsell-box-pagamento').hide();
}

/**
* Responsável por setar o meio de pagamento escolhido
*/
function setPagamentoUpsell(box){
  painel = box.closest('[data-boxpag]').attr('data-boxpag');
  sessionStorage.setItem('painelPagamento', painel);
}
/**
* Responsável por selecionar o meio de pagamento que havia sido escolhido
*/
function getPagamentoUpsell(){
  var painel = sessionStorage.getItem('painelPagamento');
  
  if(!painel) return;
  
  $('[data-menuPag="'+painel+'"]').click();
  sessionStorage.removeItem('painelPagamento');
}

/**
* Responsável por validar se um atributo foi seleciona corretamente, se nao alerta
*/
function validarAtributoSelecionado(box){
  // ALERTA ATRIBUTO NÃO SELECIONADO
  if(box.val() == 0){
    emitirAlerta(box);
    return false;
  }

  return true;
}

/**
 * Responsável por iniciar a animação de alerta no box
 */
function emitirAlerta(domElement){
  domElement.addClass('alerta');

  setTimeout(function () {
    domElement.removeClass('alerta');
  }, 1000);
}

////////////////////////////////////////////
////////////////////////////////////////////


$(document).on('click', '.upsellBotaoAdicionar', function(){
  setPagamentoUpsell($(this));
  
  var boxUpsell = $(this).closest('.upsellBox');

  // VALIDA ATRIBUTO DO PRODUTO DO UPSELL
  if(!validarAtributoSelecionado(boxUpsell.find('.upsellBoxProduto select[name="upsellAtributoSimples"]'))) return;

  // VALIDA ATRIBUTO DO BRINDE DO UPSELL
  if(!validarAtributoSelecionado(boxUpsell.find('.upsellBoxBrinde select[name="upsellAtributoSimples"]'))) return;

  adicionarUpsell($(this).attr('data-id-upsell'), boxUpsell);
});

$(document).on('click', '.upsellRemover', function(){
  removerUpsell($(this).attr('data-hash'), $(this).attr('data-tipo'));
});

$(document).on('click', '.esconderUpsells', function(){
  esconderUpsells();
});

// SÓ EXIBIR O BOX DE UPSELL APÓS CARREGAMENTO COMPLETO DA PAGINA
$(document).ready(function(){
  $('.upsell-sec').show();

  getPagamentoUpsell();
});
//DISPARA O EVENTO PERSONALIZADO DA PÁGINA
pushGtmEvent('emptyCartView');
/**
* Inicio o foundation e carrego o slider de Upsell
*/
$(document).ready(function () {
	$(document).foundation();
	carregarSliderUpsell();
});

/**
 * Aciona o evento do GTM para indicar uma opção do checkout
 * @param integer step
 * @param string checkoutOption
 */
function addCheckoutOption(step,checkoutOption){
	//VERIFICA SE O DATALAYER EXISTE
	if((typeof dataLayer == 'undefined') || !dataLayer) return false;

	//PUSH
	dataLayer.push({
		'event':'checkoutOption',
		'ecommerce':{
			'checkoutOption':{
				'actionField':{
					'step':step,
					'option': checkoutOption
				}
			}
		}
	});
}

/**
 * Método responsável por verificar o frete selecionado na tela de endereço
 */
var gtmIdFreteSelecionado = null;
function gtmVerificarFreteSelecionado(){
	//FRETE SELECIONADO
	let freteSelecionado = $('input[name="envio"]:checked');

	//VERIFICA O CAMPO
	if(!freteSelecionado) return false;

	//VERIFICA O ID DO FRETE SELECIONADO
	if(gtmIdFreteSelecionado == freteSelecionado.val()) return false;

	//ADICIONA O ID NO CACHE
	gtmIdFreteSelecionado = freteSelecionado.val();

	//CHECKOUT OPTION
	addCheckoutOption(2,"frete:"+gtmIdFreteSelecionado);
}

/**
 * Método responsável por enviar um evento personalizado do GTM
 * @param  string name
 */
function pushGtmEvent(name){
	//VERIFICA SE O DATALAYER EXISTE
	if(typeof dataLayer == 'undefined') return false;

	//DATALAYER PUSH
	dataLayer.push({
		event: name
	});
}

/**
 * Fancybox geral
 */
$('[data-fancybox]').fancybox({

});

/**
* Função responsavel pelo carregamento das imagens do checkout
*/
refreshTHB();
function refreshTHB(){
	//Use o seletor jquery para as imagens que deseja usar o placeholder.
	$('img.placeholder').each(function(){
		var img = $(this);
		var url = img.data('src');
		if(url.length == 0){
			url = img.data('default');
		}
		if(location.protocol == 'https:'){
			url = url.replace('http://','https://');
		}
		$.ajax({
			url:url,
			type:'HEAD',
			success: function() {
        if(img.hasClass('thb')){
          url = CAMINHO+'/imagem/?src='+url;
  				if(img.data('parametros')) url += img.data("parametros");
        }
			},
			error:function(e){
				url = img.data('default');
			},
			complete:function(e){
				img.animate({opacity:0},300,function(e){
					img.attr('src',url);
					img.removeClass('placeholder');
					setTimeout(function(e){img.animate({opacity:1},300);},100);
				});
			}
		});
	});
}

//MÉTODO QUE ATUALIZA OS SCRIPTS CUPOM
function atualizarScriptsCupom(){
  $('.box-cupom a[href="javascript:void(0);"]').click(function () {
    $('.box-cupom-cupons').slideToggle();
    $(this).toggleClass('open');
  });
}

//ATUALIZA OS SCRIPTS CUPOM
atualizarScriptsCupom();


/**
* Clique do botao que cancela o cupom
* @return void
*/
$(document).on('click', 'button[name="btnRemoverCupomDesconto"]', function () {
	var url = CAMINHO + '/checkout/ajax/ajax-cupom-desconto.php';
	var preloader = new Preloader();
	preloader.show('Removendo cupom...');
	jQuery.ajax({
		type: "POST",
		url: url,
		data: { 'limpar': true },
		dataType: "json",
		success: function success(data) {
			$('.cupom').html(data.layout);
			$('.boxConcluir').html(data.totalCarrinho);
			$('.produtos').html(data.produtos);
			$('.boxCarrinhoFrete .boxCarrinhoFreteOpcoes').html(data.opcoesFrete);
			$('#carrinho-brindes').html(data.alertasBrindes);
			refreshTHB();
			//ATUALIZA TOPO VENDA INTERNA
			atualizarTopoVendaInterna();
			//ATUALIZA OS DADOS DOS BRINDES
			funcBrindes();
			//ATUALIZA MENSAGENS DO CARRINHO
			atualizarMensagensCarrinho();
			// ATUALIZA LAYOUT DE PROD COM PERSONALIZAÇÃO
			ajustarLayoutPersonalizacao();

			//REMOVE PRELOADER
			preloader.hide();

			if($('input[name=cep]').val().length && !CARRINHO_B2B) $('button[name="btnCalcularFrete"]').click();

			refreshFancybox();
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);

			//REMOVE PRELOADER
			preloader.hide();
		}
	});
});

/**
* Opcao de enter do desconto cupom
*/
$(document).on('keypress', 'input[name="cupom"]' , function(e){
	if (e.which == 13) {
		$('button[name="btnCupomDesconto"]').click();
	}
});



/**
* Verificar e-mail cadastrado
*/
var timeoutValidarEmailCadastro = null;
function validarEmailCadastro(elemento) {
	clearTimeout(timeoutValidarEmailCadastro);
	var email   = $(elemento);
	var url     = CAMINHO + '/checkout/ajax/ajax-checkout-cadastro-verificar-email-checkout.php';

	jQuery.ajax({
		type: "POST",
		url: url,
		data: { 'email': email.val()},
		dataType: "json",
		success: function success(data) {
			if(!data.status){
				//VERIFICA FORMULÁRIO
				var tipoFormulario = $('#formCadastroUsuario').data('tipo-formulario');
				if(tipoFormulario == 'insert' || tipoFormulario === undefined){
					adicionarMensagemErroInputCadastro(email,data.mensagem);
				}else {
					adicionarMensagemErroInput(email,data.mensagem,false);
				}
			}
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);
		}
	});
}
$(document).on('change', '#formCadastroUsuario input[name="email"]', function (e) {
	validarEmailCadastro(this);
	return false;
});

$(document).on('keyup', '#formCadastroUsuario input[name="email"]', function (e) {
	//TECLA PRESSIONADA
	var keyCode =  e.keyCode  ? e.keyCode  :
                 e.charCode ? e.charCode :
                 e.which    ? e.which    : void 0;

	//VERIFICA SE NÃO É O ENTER
  if(keyCode != 13){
		clearTimeout(timeoutValidarEmailCadastro);
		var elemento = this;
		timeoutValidarEmailCadastro = setTimeout(function(e){validarEmailCadastro(elemento)},1000);
	}else {
		validarEmailCadastro(elemento);
	}

});


/**
* Verificar CPF/CNPJ cadastro
*/
var timeoutValidarCpfCnpjCadastro = null;
function validarCpfCnpjCadastro(elemento){
	clearTimeout(timeoutValidarCpfCnpjCadastro);
	var cpfCnpj = $(elemento);
	var url     = CAMINHO + '/checkout/ajax/ajax-checkout-cadastro-verificar-cpf-checkout.php';
	jQuery.ajax({
		type: "POST",
		url: url,
		data: { 'cpfCnpj': cpfCnpj.val()},
		dataType: "json",
		success: function success(data) {
			if(!data.status){
				//VERIFICA FORMULÁRIO
				var tipoFormulario = $('#formCadastroUsuario').data('tipo-formulario');
				if(tipoFormulario == 'insert' || tipoFormulario === undefined){
					adicionarMensagemErroInputCadastro(cpfCnpj,data.mensagem);
				}else {
					adicionarMensagemErroInput(cpfCnpj,data.mensagem,false);
				}
			}

			// BLOQUEIO DE CAMPOS PELA RECEITA
			let alterar   = data.alterarCampos;
			let nomeRazao = $('#nome-cadastro');
			let telefone  = $('#tel-fixo-cadastro');
			alterar ? nomeRazao.removeAttr('readonly'): nomeRazao.attr('readonly', true);
			alterar ? telefone.removeAttr('readonly'): telefone.attr('readonly', true);
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);
		}
	});
}

$(document).on('change', '#formCadastroUsuario input[name="cpfCnpj"]', function (e) {
	validarCpfCnpjCadastro(this);
	return false;
});

$(document).on('keyup', '#formCadastroUsuario input[name="cpfCnpj"]', function (e) {
	//TECLA PRESSIONADA
	var keyCode =  e.keyCode  ? e.keyCode  :
                 e.charCode ? e.charCode :
                 e.which    ? e.which    : void 0;

	//VERIFICA SE NÃO É O ENTER
  if(keyCode != 13){
		clearTimeout(timeoutValidarCpfCnpjCadastro);
		var elemento = this;
		timeoutValidarEmailCadastro = setTimeout(function(e){validarCpfCnpjCadastro(elemento)},1000);
	}else {
		validarCpfCnpjCadastro(elemento);
	}
});

/**
* Click do botao do desconto
*/
$(document).on('click', 'button[name="btnCupomDesconto"]', function(e){
	var cupom = $('input[name="cupom"]').val();
	var url 		= CAMINHO + '/checkout/ajax/ajax-cupom-desconto.php';
	var preloader = new Preloader();
	preloader.show('Calculando cupom...');
	jQuery.ajax({
		type: "POST",
		url: url,
		data: { cupom: cupom },
		dataType: "json",
		success: function success(data) {
			$('.cupom').html(data.layout);
			$('.boxConcluir').html(data.totalCarrinho);
			$('.produtos').html(data.produtos);
			$('.boxCarrinhoFrete .boxCarrinhoFreteOpcoes').html(data.opcoesFrete);
			$('.boxProdutoCarrinho .boxCarrinhoFreteGratis').html(data.boxFreteGratis);
			$('.mensagemCarrinho').html(data.mensagem);
			$('#carrinho-brindes').html(data.alertasBrindes);

			refreshTHB();
			//ATUALIZA OS DADOS DOS BRINDES
			funcBrindes();

			//ATUALIZA OS SCRIPTS DO CUPOM
			atualizarScriptsCupom();

			//ATUALIZA TOPO VENDA INTERNA
			atualizarTopoVendaInterna();

			//ATUALIZA MENSAGENS DO CARRINHO
			atualizarMensagensCarrinho();

			// ATUALIZA LAYOUT DE PROD COM PERSONALIZAÇÃO
			ajustarLayoutPersonalizacao();

			//REMOVE PRELOADER
			preloader.hide();

			if($('input[name=cep]').val().length && !CARRINHO_B2B) $('button[name="btnCalcularFrete"]').click();

			refreshFancybox();
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);
		}
	});
});

//INICIA O FANCYBOX
refreshFancybox();

/**
* Clique do submit para enviar os dados do form
*/
$(document).on('click', '.btnConfirmaFrete', function(){
	var idFreteAtual      = $('input[name ="idFreteAtual"]').val();
	var idFreteNovo       = $('input[name ="idFrete"]:checked').val();
	var idArmazemAnterior = $('input[name ="idArmazemAnterior"]').val();
	var idArmazemNovo     = $('input[name ="idArmazemNovo"]').val();
	var idPickup          = $('input[name ="idPickup"]:checked').val();

	if($('input[name="idFrete"]:checked').hasClass('inputPickup') && typeof idPickup == 'undefined'){
		alert('Por favor, selecione um ponto de retirada.')
		return false;
	}

	if(idFreteAtual != idFreteNovo || idArmazemAnterior != idArmazemNovo ){
		$("form[name=formOpcoesFrete]").submit();
	}else{
		parent.$.fancybox.close();
	}
})

/**
* Change dos input radio das opcoes de frete, cada mudança ele atribui valores aos
* campos de input hidden
*/
$(document).on('click', '.formulario-radio', function () {
	var idArmazemNovo     = $(this).closest('div').find('input').data('idarmazemnovo');
	var idFrete 		      = $(this).closest('div').find('input').val();
	var idFreteCotacao 	  = $(this).closest('div').find('input').data('id-cotacao');
	var nomeFrete 		    = $(this).closest('div').find('input').data('nomefrete');
	var valorFrete 		    = $(this).closest('div').find('input').data('valorfrete');
	var prazoFrete 		    = $(this).closest('div').find('input').data('prazofrete');
	var idFreteMapeamento = $(this).closest('div').find('input').data('idfretemapeamento');

	if($('.inputPickup').length > 1){
		var inputFrete = $(this).closest('div').find('input');
		inputFrete.find('input[name="idFrete"]').val(idFrete);
	}else{
		 $(this).closest('div').find('input[name="idFrete"]').val(idFrete);
	}

	// VERIFICA SE É UM FRETE DE AGENDAMENTO
	let agendamento = /agendar/i
	if(agendamento.test(idFrete)) {
		$('.abrirEntregaAgendada').attr('data-id-frete', idFrete);
		$('[name="agendamento"]').val('true');

		let multiEstoque = $(this).closest('div').find('input').attr('data-multi-estoque') !== undefined;
		let prazo        = $(this).closest('div').find('input').attr('data-selecionar-entrega-agendada');

		getPopupEntregaAgendada(parent.WapStore.pagina.secao, multiEstoque, prazo);
		return;
	}

	$('input[name="idArmazemNovo"]').val(idArmazemNovo);
	$('input[name="nomeFrete"]').val(nomeFrete);
	$('input[name="valorFrete"]').val(valorFrete);
	$('input[name="idFreteCotacao"]').val(idFreteCotacao);
	$('input[name="prazoFrete"]').val(prazoFrete);
	$('input[name="idFreteMapeamento"]').val(idFreteMapeamento);
});

/******************************************** CADASTRO DE CLIENTE *************************************/

var SPMaskBehaviorCpfCnpj = function SPMaskBehaviorCpfCnpj(val) {
	caracter = val.replace(/\D/g, '');
	length = parseInt(caracter.length);
	if (length > 11) {
		return '00.000.000/0000-00';
	} else {
		return '000.000.000-009';
	}
},
    spOptionsCpfCnpj = {
	clearIfNotMatch: true,
	onKeyPress: function onKeyPress(val, e, field, options) {
		field.maskcheckout(SPMaskBehaviorCpfCnpj.apply({}, arguments), options);
	}
};

$('.maskCheckoutCPF').maskcheckout(SPMaskBehaviorCpfCnpj, spOptionsCpfCnpj);

$(document).on('keyup', '.maskCheckoutCPF', function () {
	var caracter, element;
	element = $(this);
	caracter = element.val().replace(/\D/g, '');
	$(this).parent().find('.formError').remove();
	element.removeClass('validate[required]');
	if (caracter.length > 11) {
		element.removeClass('validate[required,custom[cpf]]');
		element.addClass('validate[required,custom[cnpj]]');
		mudarParametrosCadastroCliente('juridica');
		gerenciarPermissaoClientePessoa('juridica');
	} else {
		element.removeClass('validate[required,custom[cnpj]]');
		element.addClass('validate[required,custom[cpf]]');
		mudarParametrosCadastroCliente();
		gerenciarPermissaoClientePessoa();
	}
}).trigger('keyup');

/**
 * Função responsável por gerenciar a visibilidade das informações adicionais
 * @param  {string} clientePessoa
 * @return {void}
 */
function gerenciarPermissaoClientePessoa(clientePessoa = 'fisica'){
	$('.informacoesAdicionaisCadastroCliente input, .informacoesAdicionaisCadastroCliente select, .informacoesAdicionaisCadastroCliente textarea').each(function(){
		let domInformacaoAdicional 	= $(this).closest('.boxCampoCheckoutCadastro');
		let permissaoPessoaFisica 	= $(this).attr('data-permissao-pessoa-fisica');
		let permissaoPessoaJuridica = $(this).attr('data-permissao-pessoa-juridica');

		if(typeof permissaoPessoaFisica   === 'undefined') return;
		if(typeof permissaoPessoaJuridica === 'undefined') return;

		switch (clientePessoa) {
			case 'fisica':
				(permissaoPessoaFisica == 'true') ? (domInformacaoAdicional.show()) : (domInformacaoAdicional.hide());
				break;
		
			default:
				(permissaoPessoaJuridica == 'true') ? (domInformacaoAdicional.show()) : (domInformacaoAdicional.hide());
				break;
		}
	});
}

// AÇÃO PARA AJUSTAR OS CAMPOS E MÁSCARA NO CHECKOUT CADASTRO
$(document).ready(function() {
	var campo                   = $('.maskCheckoutCPF');
	var tipoCadadastroPermitido = campo.data('tipo-cadastro-permitido');

	if(typeof tipoCadadastroPermitido === "undefined" || tipoCadadastroPermitido == 'todos') return;

	campo.removeClass('maskCheckoutCPF validate[required]');

	switch (tipoCadadastroPermitido) {
		case 'cpf':
			campo.addClass('mascaraCPFPessoaFisica validate[required,custom[cpf]]');
			mudarParametrosCadastroCliente();
			break;

		case 'cnpj':
			campo.addClass('mascaraCPFPessoaJuridica validate[required,custom[cnpj]]');
			mudarParametrosCadastroCliente('juridica');
			break;
	}
});

