/**
* Função que verifica se o cumpom está ativo, Se não tiver limpa a mensagem
* @method verificarCupomAtivo
*/
function verificarCupomAtivo() {
	var url = CAMINHO + '/checkout/ajax/ajax-cupom-desconto.php';
	var preloader = new Preloader();
	preloader.show('Verificando cupom...');
	jQuery.ajax({
		type: "POST",
		url: url,
		data: { 'verificar': true },
		dataType: "json",
		success: function success(data) {
			$('.boxConcluir').html(data.totalCarrinho);
			$('.boxResumoTotalizacao').html(data.resumoTotalizacao);

			if(WapStore.pagina.subsecao == 'carrinho') $('.produtos').html(data.produtos);

			// CARREGA AS IMAGENS DOS PRODUTOS
			refreshTHB();

			//VERIFICA LAYOUT DO CUPOM
			if (data.layout) {
				$('.cupom').html(data.layout);

				//ATUALIZA OS SCRIPTS DO CUPOM
				atualizarScriptsCupom();
			}
			//ATUALIZA TOPO VENDA INTERNA
			atualizarTopoVendaInterna();
			//ATUALIZA OS DADOS DOS BRINDES
			funcBrindes();
			//ATUALIZA MENSAGENS DO CARRINHO
			atualizarMensagensCarrinho();
			//REMOVE PRELOADER
			preloader.hide();
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);
		}
	});
}

/**
 * RESPONSÁVEL POR RETORNAR O RESULTADO DO CEP
 */
$(document).on('submit', '.formularioBuscaCep', function () {
	var uf = $('.uf').val();
	var cidade = $('.cidade').val();
	var rua = $('.rua').val();
	$.ajax({
		type: 'POST',
		url: CAMINHO + '/checkout/ajax/ajax-nao-sei-meu-cep.php',
		dataType: 'json',
		data: {
			uf: uf,
			cidade: cidade,
			rua: rua
		},
		success: function success(data) {
			//APRESENTA RESULTADO
			$('.right').html(data);

			//ROLA A TELA PARA O RESULTADO DA BUSCA
			$('html, body').animate({
				scrollTop: $('.right').offset().top
			}, 500);
		},
		error: function error(data) {}
	});
	return false;
});

/**
 * EXECUTADO QUANDO O CLIENTE CLICAR PARA USAR O CEP ENCONTRADO
 */
$(document).on('click', '.usar-cep', function () {
	var cep = $(this).attr('data-cep');
	parent.$('input[name="cep"]').val(cep);
	parent.$('.calcularFrete').trigger('click');
	parent.$('#cepAutoComplete').trigger('change');

	//ATUALIZA A VALIDAÇÃO
	parent.$('#cepAutoComplete').blur();

	//ATUALIZA O LABEL DO CAMPO CEP
	var label = parent.$('#cepAutoComplete').closest('.mainBox-conteudo-form-input').find('.mainBox-conteudo-form-input-label');
	label.removeClass('colorir');
	label.addClass('inpAtivo');

	parent.$.fancybox.close();
});

//FORMULÁRIO DE RECOMENDAÇÃO
$(document).on('submit','.formularioRecomendacao',function(event){
  event.preventDefault();

	var preloader = new Preloader;
  preloader.show('Salvando alterações...');

  var recomendacao = $('#recomendacao').val();
  var idPedido     = $('#idPedido').val();

	$.ajax({
		type: 'POST',
		url: CAMINHO + '/checkout/ajax/ajax-recomendacao.php',
		dataType: 'json',
		data: {
			recomendacao: recomendacao,
			idPedido    : idPedido
		},
		success: function success(response){
		var titulo = (response.status == 'success') ? 'Sucesso!' : 'Falha!';
				setTimeout(function(){
					preloader.hide();
					swal({'title': titulo, 'text':response.mensagem, 'type':response.status,'confirmButtonText': 'OK','confirmButtonColor': '#24a64c'},
						function(confirm){
							if((response.status == 'success') && confirm){
								parent.$('.fancybox-overlay').click();
							}
						}
					);
				}, 500);
			},
	});
	return false;
});

/**
 * Função responsável por chamar o ajax de alteração do preço do item do carrinho
 * @param {Number} precoAlterado Novo preço do item
 * @param {String} hashProduto   Hash do item do carrinho
 * @param {Number} idProduto     ID do produto
 * @param {String} tipoEdicao    Tipo de edição sofrinda pelo item do carrinho
 */
function alterarPrecoProduto(precoAlterado, hashProduto, idProduto, tipoEdicao) {
  var preloader = new Preloader;
  preloader.show('Alterando preço...');

  $.ajax({
    type: 'POST',
	  url: CAMINHO + '/checkout/ajax/ajax-editar-preco.php',
		dataType: 'json',
		data: {
	  	hashProduto : hashProduto,
	  	idProduto   : idProduto,
	  	preco       : precoAlterado,
	  	tipoEdicao  : tipoEdicao
		},
		success: function success(response){
	  	setTimeout(function() {
				window.location.reload();
	  	}, 500);
		},
  });
};

/**
 * Funcao valida o cpf
 * @param  string strCPF CPF informado
 * @return bolean Retorna true or false
 */
function testarCPF(strCPF) {

	strCPF = strCPF.replace(/[^0-9]/g, '');

	var Soma;
	var Resto;
	Soma = 0;
	if (strCPF == "00000000000") return false;

	for (i = 1; i <= 9; i++) {
		Soma = Soma + parseInt(strCPF.substring(i - 1, i)) * (11 - i);
	}Resto = Soma * 10 % 11;

	if (Resto == 10 || Resto == 11) Resto = 0;
	if (Resto != parseInt(strCPF.substring(9, 10))) return false;

	Soma = 0;
	for (i = 1; i <= 10; i++) {
		Soma = Soma + parseInt(strCPF.substring(i - 1, i)) * (12 - i);
	}Resto = Soma * 10 % 11;

	if (Resto == 10 || Resto == 11) Resto = 0;
	if (Resto != parseInt(strCPF.substring(10, 11))) return false;
	return true;
}

$('.pagamentoSucesso-boxNumero a').click(function(e){
  link = $(this).attr('href');

  if(link == 'javascript:void(0);') return false;
});

//VALIDA CAMPOS PREENCHIDOS
validarCamposPreenchidos();
function validarCamposPreenchidos(){
	refreshValidation();
	$('.mainBox-conteudo-form-input-validacao input, .boxCheckout-newAdress input, .boxCheckout-newAdressCadastro input').not('.mainBox-conteudo-form-input-validacao-recaptcha, #cepAutoComplete').each(function(){
		var valorInput = $(this).val();
		var label      = $(this).closest('.mainBox-conteudo-form-input').find('.mainBox-conteudo-form-input-label');

		//VERIFICA INPUT
	  if(valorInput != "") {

			//ATUALIZA O LABEL
			label.addClass('inpAtivo');

			//ATUALIZA VALIDATE
			$(this).blur();
		}

	});
}


/**
* Clique do botao para alternar o tipo de faturamento
*/
$(document).on('change', 'input[name="idPagamentoFaturamento"]', function(e){
	calcularTaxaFaturamento();
})

/**
* Função para calcular a taxa por prazo no faturamento
* @return layout lista de faturamentos
*/
function calcularTaxaFaturamento(){

	//MONTAR VARIAVEIS PARA O CARRINHO
  var valorTotal                 = parseFloat(PED_TOTAL);
	var dadosFaturamento           = $('input[name="idPagamentoFaturamento"]:checked');
  var idFaturamento   		       = dadosFaturamento.val();
	var taxa                       = dadosFaturamento.data('taxa');


  var preloader = new Preloader();
	preloader.show('Calculando Faturamento...');

	var url   = CAMINHO+'/checkout/ajax/ajax-calcular-faturamento.php';
	jQuery.ajax({
		type: "POST",
		url: url,
		data : {
			idFaturamento,
			taxa

		},
		dataType:"json",
		success: function( data ){

      taxaValor = data.taxaValor;

      labelFaturamento = data.labelFaturamento;

      valorTotal = data.totalGeral;

      $('#dadosFaturamento').show();

      $('#labelFaturamento').html(labelFaturamento);

      $('#valorTotalFaturamento').html('R$ ' + number_format(taxaValor,2,',','.'));

      $('#valorTotalPedido, #valorPedidoExterno').html('R$ ' + number_format(valorTotal,2,',','.'));

    	preloader.hide();

			//ATUALIZA TOPO VENDA INTERNA
			atualizarTopoVendaInterna();

			//ATUALIZA MENSAGENS DO CARRINHO
			atualizarMensagensCarrinho();

			//VERIFICA SE POSSUI CUPOM ATIVO
			// verificarCupomAtivo();
		},
		error:function(data){
			//mensagem de erro
			console.log(data);
			preloader.hide();
		}
	});
}

/**
 * RESPONSÁVEL POR TROCAR O ENQUADRAMENTO FISCAL
 */
 $(document).on('submit', '.formularioEnquadramento', function (e) {
	e.preventDefault();
	let enquadramento = $('input[name="enquadramento"]:checked').val();
	if(typeof enquadramento != 'undefined'){
		$.ajax({
			type: 'POST',
			url: CAMINHO + '/checkout/ajax/ajax-fiscal-enquadramento.php',
			dataType: 'json',
			data: {
				enquadramento: enquadramento
			},
			success: function success(data) {
				if(data.sucesso == true){
					parent.$.fancybox.close();
					parent.window.location = CAMINHO+'/checkout/carrinho';
				}
			}
		});
	}
});

//SELECT DE ENQUADRAMENTO FISCAL
if($('select[name="enquadramento"]').data('mostrar') == 'hide') $('select[name="enquadramento"]').closest('.boxCampoCheckoutCadastro').hide();

if((typeof $('input[name="cpfCnpj"]').val() != 'undefined') && $('input[name="cpfCnpj"]').val().length > 14){
	let enquadramento = $('select[name="enquadramento').data('enquadramento');
	if(enquadramento === 'f' || !enquadramento) enquadramento = 0;
	$('select[name="enquadramento').removeProp('selected');
	$('select[name="enquadramento"] option[value="'+enquadramento+'"]').prop('selected','selected');
}

/**
* Atualiza fancybox das popups
*/
function refreshFancybox() {
	$(".abrePopup").fancybox({
		autoScale: true,
		transitionIn: 'none',
		transitionOut: 'none',
		type: 'iframe',
		maxWidth: '840px',
		width: '100%',
		padding: 0,
		afterShow: function afterShow() {
			document.body.style.overflow = 'hidden';
		},
		afterClose: function afterClose() {
			document.body.style.overflow = 'auto';
		}
	});

	//POPUP DE RECOMENDACAO
	$('.abrirPopupRecomendacao').fancybox({
	autoScale: true,
	transitionIn: 'none',
	transitionOut: 'none',
	maxWidth: '460px',
	width: '100%',
	wrapCSS : 'fancyboxFormularioRecomendacao',
	type: 'iframe',
	closeBtn: 'true',
	helpers: {
	  overlay: {
		locked: false
	  }
	},
	afterShow: function afterShow() {
	  document.body.style.overflow = 'hidden';
	},
	afterClose: function afterClose() {
	  document.body.style.overflow = 'auto';

    // ABRE A POP-UP DE SELEÇÃO DEPOIS DE FECHAR A POP-UP DE RECOMENDAÇÃO.
    if($('#selecionarMeiosPagamentoLink').length) {
      $("#selecionarMeiosPagamentoLink").trigger('click');
    }
	}
  });

  // ABRE POPUP DE SELEÇÃO DE PAGAMENTO POR EMAIL.
  $('.abrirPopupSelecaoPagamento').fancybox({
    autoScale: true,
    transitionIn: 'none',
    transitionOut: 'none',
    wrapCSS : 'fancyboxFormularioSelecaoPagamento',
    maxWidth: '500px',
    width: '100%',
    type: 'iframe',
    closeBtn: 'true',
    helpers: {
      overlay: {
        locked: false
      }
    },
    afterShow: function afterShow() {
      document.body.style.overflow = 'hidden';
    },
    afterClose: function afterClose() {
      document.body.style.overflow = 'auto';
    }
  });

  $(".abrePopupNegociacao").fancybox({
		autoScale: true,
		transitionIn: 'none',
		transitionOut: 'none',
		padding: '0',
		maxWidth: '400px',
		width: '100%',
		wrapCSS : 'fancyboxFormularioCheckoutNegociacao',
		type: 'iframe',
		closeBtn: false,
		helpers: {
		  overlay: {
				closeClick: false,
		    locked: false
		  }
		},
		afterShow: function afterShow() {
		  document.body.style.overflow = 'hidden';
		},
		afterClose: function afterClose() {
		  document.body.style.overflow = 'auto';
		}
  });
}

