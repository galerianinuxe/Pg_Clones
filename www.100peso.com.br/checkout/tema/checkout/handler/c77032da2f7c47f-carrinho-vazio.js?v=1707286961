$('.mascaraValor').maskcheckout('00000000000000000.00', { reverse: true });
//MASCARA DE REAL
$('.mascaraReal').maskcheckout('000.000.000.000.000,00', { reverse: true });
//MASCARA DE PORCENTAGEM
$('.mascaraPorcentagem').maskcheckout('00000000000000000,00%', { reverse: true });
//MASCARA DE PESOS
$('.mascaraPeso').maskcheckout('0000000.000', { reverse: true });
//MASCARA DE MEDIDAS
$('.mascaraMedida').maskcheckout('000000000000000.00', { reverse: true });
//MASCARA DE CPF
$('.mascaraCPF').maskcheckout('000.000.000-00');
//MASCARA DE CNPJ
$('.mascaraCNPJ').maskcheckout('00.000.000/0000-00');
// MASCARA DE CPF PARA USAR EM CONJUNTO COM A MASCARA maskCheckoutCPF
$('.mascaraCPFPessoaFisica').maskcheckout('000.000.000-00', { clearIfNotMatch: true });
// MASCARA DE CNPJ PARA USAR EM CONJUNTO COM A MASCARA maskCheckoutCPF
$('.mascaraCPFPessoaJuridica').maskcheckout('00.000.000/0000-00', { clearIfNotMatch: true });

//MASCARA CPF E CNPJ
var SPMaskBehaviorCpfCnpj = function (val) {
	caracter = val.replace(/\D/g,'');
	length   = parseInt(caracter.length);
	if(length > 11) {
		return '00.000.000/0000-00';
	}else{
		return '000.000.000-009';
	}
},
spOptionsCpfCnpj = {
	clearIfNotMatch: true,
	onKeyPress: function(val, e, field, options) {
		field.maskcheckout(SPMaskBehaviorCpfCnpj.apply({}, arguments), options);
	}
};
$('.mascaraCpfCnpj').maskcheckout(SPMaskBehaviorCpfCnpj, spOptionsCpfCnpj);
$(document).on('keyup','.mascaraCpfCnpj', function(){
	var caracter, element;
	element = $(this);
	caracter = element.val().replace(/\D/g, '');
	$(this).parent().find('.formError').remove();
	if(caracter.length > 11){
		element.removeClass('validate[required,custom[cpf]]');
		element.addClass('validate[required,custom[cnpj]]');
	}else{
		element.removeClass('validate[required,custom[cnpj]]');
		element.addClass('validate[required,custom[cpf]]');
	}
}).trigger('keyup');


