$(document).on('click','.opcaoRecuperacaoSenhaSimplificada',function(e){
  e.preventDefault();
  $('.boxRecuperacaoSenhaSimplificada').slideDown();
  $('.emailRecuperacaoSenhaSimplificada').focus();
  $('.maskCheckoutCPF').maskcheckout(SPMaskBehaviorCpfCnpj, spOptionsCpfCnpj);
  $(".maskCheckoutData").maskcheckout('00/00/0000', { clearIfNotMatch: true });
});

// ATUALIZA STATUS DO BOTÃO ENVIAR
$(document).on('keyup focus focusout','.boxCamposRecuperacaoSimplificada input',function(){
  var formulario = $(this).closest('.boxCamposRecuperacaoSimplificada');
  atualizarStatusBotaoRecuperacaoSimplificada(formulario);
});
$(document).on('mouseover','.boxCamposRecuperacaoSimplificada', function(){
  atualizarStatusBotaoRecuperacaoSimplificada($(this));
});

// RECAPTCHA RECUPERAÇÃO
function carregarReCaptachaRecuperacao(){
  if($('#reCaptchaRecuperacaoSenha').hasClass('recaptchaRecuperacaoSenha')){
    reCaptchaRecuperacaoSenhaId = grecaptcha.render("reCaptchaRecuperacaoSenha", {
      "sitekey" : "6LcxYw0UAAAAAA6km8VTfugzswcmyDbYIDxtUQ7b"
    });
  }
}
