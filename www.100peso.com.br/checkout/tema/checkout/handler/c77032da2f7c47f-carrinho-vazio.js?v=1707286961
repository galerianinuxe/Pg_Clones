});
$(document).on('focus', '#parcelasSegundoCartao', function () {
	$(this).closest('#formPagamentoDoisCartaoCredito').find('[name="cartao2[numero]"]').validationEngine('validate');
});

/**
 * SCRIPT DE ALTERAÇÃO DE MEIOS DE PAGAMENTO POR EMAIL.
 */
$('.boxConfirmarMeioPagamento').click(function(){
  let elementoPopupMeiosPagamento = $(document).find('.containerConfigurarPagamentos');

  let meiosPagamento        = {};
  let idPagamentoEmail      = elementoPopupMeiosPagamento.find('button').data('id-pagamento-email');
  let cartaoCreditoParcelas = elementoPopupMeiosPagamento.find('[name="numeroParcelas"]').val();
  elementoPopupMeiosPagamento.find('[name="exibicaoMeioPagamento[]"]').each(function(){
    if($(this).prop('checked')) meiosPagamento[$(this).val()] = $(this).val() == 'cartao' ? cartaoCreditoParcelas : 1;
  });
  let formaExibicaoMix =  $('[name="formaExibicaoCheckout"]').val();

  if(idPagamentoEmail > 0 && Object.keys(meiosPagamento).length > 0)
    atualizarMeiosPagamento(idPagamentoEmail,meiosPagamento,formaExibicaoMix);
});

/**
 * Função responsável por atualizar os meios de pagamentos que estarão disponíveis no checkout de pagamento por e-mail.
 * @param {int}    idPagamentoEmail Id do pagamento por email.
 * @param {string} meiosPagamento   Meios de pagamentos selecionados(cartao,pix,boleto,deposito e outros)
 * @param {string} formaExibicaoMix Forma de exibição do mix
 */
function atualizarMeiosPagamento(idPagamentoEmail, meiosPagamento, formaExibicaoMix){
  let url = CAMINHO+'/checkout/ajax/pagamento-email/ajax-checkout-pagamento-email-selecionar-meios-pagamento.php';

  let preloader = new Preloader();
  preloader.show('Atualizando meios de pagamento');

  jQuery.ajax({
    url,
    type: 'POST',
    dataType: 'json',
    data: { idPagamentoEmail, meiosPagamento, formaExibicaoMix }
  }).done(function(data){
    if(data.status)
      $(window.top.document).find('.tabela-tipos-pagamento tbody').html(data.itensTabela);

    preloader.hide();
    top.$.fancybox.close();
  }).fail(function(data){
    preloader.hide();
    top.$.fancybox.close();

    console.log(data);
  });
}

// ALTERAÇÃO DE ATIVAÇÃO DE SELECT DE PARCELAS DE CARTÃO EM POP-UP DE SELEÇÃO DE PAGAMENTOS.
$('.alinha-label input[value="cartao"]').click(function() {
  if ($(this).is(':checked')) {
    $('.selectOptionParcelas').attr('disabled', false);
    return;
  }

  $('.selectOptionParcelas').attr('disabled', true);
})

/**
 * Ação responsável por chamar a alteração de preço do item do carrinho
 */
$(document).on('change', 'input[name="precoAlterado"]', function(){
  var precoAlterado = $(this).val();
  var hashProduto   = $(this).data('hashproduto');
  var idProduto     = $(this).data('idproduto');
  var tipoEdicao    = $(this).data('tipoedicao');

  if(precoAlterado !== ''){
    alterarPrecoProduto(precoAlterado, hashProduto, idProduto, tipoEdicao);
  }
});

/**
 * Ação responsável por restaurar a máscara ao input
 */
$(document).on('click', 'input[name="precoAlterado"]', function(){
  //MASCARA DE VALORES
  $(this).maskcheckout('00000000000000000.00', { reverse: true });
});

/**
 * Função responsável por abrir a popup de alteração de armazém de um produto
 * @param {string} urlPopup URL da popup com a query string
 */
function abrirAlteracaoArmazem(urlPopup) {
  var elementoAbrePopup = $('.abrePopupAlterarArmazem');
  elementoAbrePopup.attr('href', urlPopup);
  elementoAbrePopup.click();
}

/**
 * Função responsável por tratar o retorno do ajax-processar-venda
 * @param {Object} retorno   Retorno do ajax-processar-venda
 * @param {Object} preloader Objeto de preloader
 */
function tratarRetornoProcessarVenda(retorno, preloader) {
	switch (true) {
		case !retorno.sucesso && (retorno.url != null && retorno.url.length > 0) && (retorno.erro != null && retorno.erro.length > 0):
			preloader.hide();
			emitirAlertaProcessarVenda(retorno.erro, retorno.url);
			break;

		case retorno.sucesso || (retorno.url != null && retorno.url.length > 0):
			window.location = retorno.url;
			break;
	
		default:
			preloader.hide();
			emitirAlertaProcessarVenda(retorno.erro + '<br/>Para manter a integridade do sistema a página será recarregada e os dados atualizados.', window.location);
			break;
	}
}

/**
 * Função responsável por emitir um alerta no checkout
 * @param {String} mensagem    Mensagem de alerta
 * @param {String} urlRedirect URL de redirecionamento
 */
function emitirAlertaProcessarVenda(mensagem, urlRedirect) {
	var alertCustomVars = {
		title: "Erro!",
		text: mensagem,
		type: "error",
		confirmButtonText: "Ok",
		allowEscapeKey: false,
		html: true
	};

	alertCustom(alertCustomVars, function (e) {
		window.location = urlRedirect;
	});
}/**
* Clique do botao de quantidade
*/
var intervalo;
$(document).on('click', '.btnCarrinhoAcao', function(){
	clearInterval(intervalo);

	var boxQuantidade     = $(this).closest('.boxCheckoutCarrinhoProdutoItem').find('.boxProdutoCarrinhoQuantidade');
	var inputQuantidade   = boxQuantidade.find('.quantidadeProdutoCarrinho');
	var quantidade        = inputQuantidade.val();
	var idProduto         = inputQuantidade.data('id');
	var idAtributoSimples = inputQuantidade.data('atributo');
	var idUnidadeVenda    = inputQuantidade.data('unidade-venda');
	var hash              = inputQuantidade.data('hash');
	var idArmazem         = inputQuantidade.data('armazem');
	var acao              = $(this).data('acao');
	var tipo              = $(this).data('tipo');

	quantidade = !isNaN(quantidade) ? parseInt(quantidade) : 1;

	switch (acao) {
		case 'subtrair':
		quantidade = quantidade > 1 ? quantidade - 1 : 1;
		acao       = 'atualizar';
		break;
		case 'adicionar':
		quantidade = quantidade + 1;
		acao       = 'atualizar';
		break;
	}
	inputQuantidade.val(quantidade);


	intervalo = setTimeout(function(e){
		carrinhoAcao(idProduto,idAtributoSimples,idUnidadeVenda,hash,idArmazem,tipo,acao,quantidade);
	}, 500);
});


/**
 * Change do input da quantidade do produto no carrinho
  */
$(document).on('change','.quantidadeProdutoCarrinho',function(e){
	var idProduto         = $(this).data('id');
	var idAtributoSimples = $(this).data('atributo');
	var idUnidadeVenda    = $(this).data('unidade-venda');
	var idArmazem         = $(this).data('armazem');
	var hash              = $(this).data('hash');
	var quantidade        = $(this).val();
	var tipo              = $(this).data('tipo');

	if(isNaN(quantidade)){
		quantidade = 1;
	}else if(quantidade <= 0){
		quantidade = 1;
	}else{
		quantidade = parseInt(quantidade);
	}
	$(this).val(quantidade);
	carrinhoAcao(idProduto,idAtributoSimples,idUnidadeVenda,hash,idArmazem,tipo,'atualizar',quantidade);
});



/**
* Executa as ações do carrinho
*/
function carrinhoAcao(idProduto,idAtributoSimples,idUnidadeVenda,hash,idArmazem,tipo,acao,quantidade){
	var url   = CAMINHO+'/checkout/ajax/ajax-carrinho-acao.php';
	var preloader = new Preloader();
	preloader.show('Carregando...');

	jQuery.ajax({
		type: "POST",
		url: url,
		data : {
			'idProduto':idProduto,
			'idAtributoSimples':idAtributoSimples,
			'idUnidadeVenda':idUnidadeVenda,
			'hash':hash,
			'idArmazem':idArmazem,
			'tipo':tipo,
			'acao':acao,
			'quantidade':quantidade
		},
		dataType:"json",
		success: function( data ){

			//VERIFICA ALTERAÇÕES NOS ESTOQUES DOS ITENS
			if(data.statusAlteracaoEstoque){
				window.location = CAMINHO+'/checkout/carrinho/?acao='+data.hashAcaoEstoque;
			}else{

				//VERIFICA LAYOUT
				if(data.layout !== null){
					$('.produtos').html(data.layout);
					$('.boxConcluir').html(data.totalCarrinho);
					$('.boxCarrinhoFrete .boxCarrinhoFreteOpcoes').html(data.opcoesFrete);
					$('.boxProdutoCarrinho .boxCarrinhoFreteGratis').html(data.boxFreteGratis);
					$('.mensagemCarrinho').html(data.mensagem);
					$('#carrinho-brindes').html(data.alertasBrindes);
					$('.vitrine-carrinho').html(data.vitrine);
					$('.upsell-sec').html(data.upsell);

					//VERIFICA COLUNA EMBALAGEM PRESENTE
					if(data.statusPresente && data.entrega){
						$('.boxProdutoCarrinho .boxProdutoCarrinhoPresente').show();
					}else{
						$('.boxProdutoCarrinho .boxProdutoCarrinhoPresente').hide();
					}

					//VERIFICA ENTREGA
					if(data.entrega){
						//MOSTRA BOX DE FRETES
						$('.boxProdutoCarrinho .boxCarrinhoFrete').show();
					}else{
						//ESCONDE BOX DE FRETES
						$('.boxProdutoCarrinho .boxCarrinhoFrete').hide();
					}

					// VITRINE
					if(data.vitrine) carregarVitrineCarrinho();

					// UPSELL
					if(data.upsell) carregarSliderUpsell();

					refreshTHB();

					funcBrindes();
					
					// ATUALIZA LAYOUT DE PROD COM PERSONALIZAÇÃO
	        ajustarLayoutPersonalizacao();

					//ATUALIZA TOPO DA VENDA INTERNA
					atualizarTopoVendaInterna();

					//ATUALIZA MENSAGENS DO CARRINHO
					atualizarMensagensCarrinho();

					//VERIFICA SE POSSUI CUPOM ATIVO
					if(CARRINHO_B2B != true) verificarCupomAtivo();

					//ADICIONA OS DATALAYERS DO EVENTO
					if(data.datalayer && (typeof dataLayer != "undefined")){
						dataLayer.push(data.datalayer);
					}

					if(data.dataLayerViewCart && (typeof dataLayer != "undefined")){
						dataLayer.push(data.dataLayerViewCart);
					}

					// INICIALIZA O PLUGIN AO ATUALIZAR A PÁGINA 
					$(document).foundation();

					refreshFancybox();

					preloader.hide();
				}else{

					if(data.dataLayerViewCart && (typeof dataLayer != "undefined")){
						dataLayer.push(data.dataLayerViewCart);
					}

					if(data.datalayer && (typeof dataLayer != "undefined")){
						//ADICIONA OS DATALAYERS DO EVENTO
						dataLayer.push(data.datalayer);

						//AGUARDA ALGUNS MILISSEGUNDOS PARA RECARREGAR A PÁGINA
						setTimeout(function () {
							window.location = CAMINHO+'/checkout/carrinho';
						}, 300);
					}else{
						window.location = CAMINHO+'/checkout/carrinho';
					}
				}

				if(data.opcoesFrete !== null) dataLayer.push({"event": "shipping_calculation"});
			}
		},
		error:function(data){
			console.log(data);
		}
	});
}

$(document).ready(function(){

  funcBrindes();

});

function updateAttrUnico(objectSelector) {

  id            = $(objectSelector).data('idproduto');
  selecionado   = $(objectSelector).find('option:selected').val();
  idBrinde      = $(objectSelector).closest('.box-brinde').attr('data-idBrinde');
  key           = $(objectSelector).closest('.box-brinde').attr('data-key');

  if(selecionado == 0) return funcBrindes();

  var preloader = new Preloader();
	preloader.show('Atualizando brinde...');

  $.ajax({
        type:'POST',
        url: CAMINHO+'/checkout/ajax/brinde/ajax-produto-brinde.php',
        dataType:'json',
        data:{
          idProdutoLider: id,
          idProdutoSelecionado: selecionado,
          idBrinde: idBrinde,
          key: key
        },
        success:function(data){
          box = $(objectSelector).closest('.box-brinde').closest('.small-12.columns');

          box.after(data.box);

          box.remove();

          refreshTHB();

          try {
            funcBrindes();
          } catch (error) {
            preloader.hide();
          }

          preloader.hide();
        },
        error:function(data){
          preloader.hide();
        }
      });

}

function updateAttrSimples(objectSelector) {

  id            = $(objectSelector).data('idproduto');
  selecionado   = $(objectSelector).find('option:selected').val();
  key           = $(objectSelector).closest('.box-brinde').attr('data-key');
  idBrinde      = $(objectSelector).closest('.box-brinde').attr('data-idBrinde');

  var preloader = new Preloader();
	preloader.show('Atualizando brinde...');

  $.ajax({
        type:'POST',
        url: CAMINHO+'/checkout/ajax/brinde/ajax-produto-brinde-atributo-simples.php',
        data:{
          idProdutoLider:id,
          idAttrValor:selecionado,
          idBrinde:idBrinde,
          key:key
        },
        success:function(data) {
          funcBrindes(false);
          preloader.hide();
        },
        error:function(data){
          preloader.hide();
        }
      });
}

function funcBrindes(ativarSlide = true) {
  $('select[name="attr-unico"]').unbind();
  $('select[name="attr-simples"]').unbind();

  $('select[name="attr-unico"]').change(function(){
    updateAttrUnico($(this));
  });

  $('select[name="attr-simples"]').change(function(){
    updateAttrSimples($(this));
  });

  if(hasBrinde()){
    if(brindesIncompletos()){
      bloquearCheckout();
    } else {

      addBrindeCesta();

      desbloquearCheckout();
    }

    //BRINDES MANUAL
    if(ativarSlide){
      $('.box-brinde-slider').not('.slick-initialized').slick();
      
      $('[data-select-brinde]').attr('onclick', 'selecionarBrindeCesta(this)');

      $('.removerBrindeSelecionado').click( function () {
        removerBrindeSelecionado($(this));
      });
    }
  }else {
    desbloquearCheckout();
  }
}

function addBrindeCesta() {

  $.ajax({
        type:'GET',
        url: CAMINHO+'/checkout/ajax/brinde/ajax-produto-brinde-adicionar-carrinho.php',
        dataType:'json',
      });

}

function brindesIncompletos(adicionarClasseErro = false) {
  retorno = false;

  $('.box-brinde').each(function(e, i) {
    attrSimples   = $(this).find('select[name="attr-simples"]');
    attrUnico     = $(this).find('select[name="attr-unico"]');
    //REMOVE ALERTA DE BRINDE
    $('.boxCarrinhoAvisoBrinde').hide();

    //REMOVE A CLASSE DE ERRO DAS OPÇÕES
    $(this).removeClass('escolhaAtributos');

    if(attrSimples.length > 0){
      if(attrSimples.find('option:selected').val() == 0){
        if(adicionarClasseErro) $(this).addClass('escolhaAtributos');
        retorno = true;
      }
    }

    if(attrSimples.length > 0){
      if(attrSimples.find('option:selected').val() == 0){
        if(adicionarClasseErro) $(this).addClass('escolhaAtributos');
        retorno = true;
      }
    }

    //BLOQUEIA CASO EXISTA BRINDES A SEREM SELECIONADOS
    boxSlideBrinde = $(this).find('.box-brinde-slider');
    if(boxSlideBrinde.length > 0){
      retorno =  true;
    }
  });

  //MOSTRA ALERTA DE BRINDE
  if(retorno && adicionarClasseErro) $('.boxCarrinhoAvisoBrinde').show();

  return retorno;
}

function desbloquearCheckout() {
  $('.boxCarrinhoFrete input').removeAttr('disabled');
  $('.botaoFinalizarCompra').removeClass("disabledLink");
  $('.boxCarrinhoFrete').removeClass('boxCarrinhoFreteInativo');
  $('.boxCarrinhoFrete #calcularFrete').removeClass('disabledLink');
}

function bloquearCheckout() {
  $('.boxCarrinhoFrete input').attr('disabled', true);
  $('.botaoFinalizarCompra').addClass("disabledLink");
  $('.boxCarrinhoFrete').addClass('boxCarrinhoFreteInativo');
  $('.boxCarrinhoFrete #calcularFrete').addClass('disabledLink');
}

function hasBrinde() {
  qtdBrindes = $('.box-brinde').length;

  return qtdBrindes > 0;
}

//CHAMA ATENÇÃO AS OPÇÕES DE BRINDE
$(document).on('click','.disabledLink',function(event){
  event.preventDefault();
  //ROLA A PÁGINA
  var distancia = $('.box-brinde').offset().top;
  $('html, body').animate({'scrollTop':(distancia - 50)});

  //VERIFICA BRINDES INCOMPLETOS
  brindesIncompletos(true);

  return false;
});
// NEGOCIAÇÃO

// VARIÁVEL QUE DETERMINA SE A TELA SERÁ RECARREGADA AO FECHAR A POPUP
let recarregarTela = false;

/**
 * Função responsável por obter o valor do subtotal original
 */
function getValorOriginalSubtotal() {
  return $(document).find('.formularioNegociacao').data('subtotal-original');
}

/**
 * Função responsável por obter o tipo de variação da negociação
 */
function getTipoVariacao() {
  return $('input[name="tipoVariacao"]:checked').val();
}

//CLIQUE NO BOTÃO DE ABRIR EDIÇÃO DO SUBTOTAL
$('.abrirEdicaoSubtotal').on('click', function () {
  $(document).find('.negociacaoSubtotalValorVariacao').hide();
  $(document).find('.negociacaoSubtotalVariacao').show();
  $('.inputSubtotalVariacao').show();
  $(document).find('.negociacaoPercentualVariacao').show();
  $(document).find('.negociacaoValorVariacao').show();
  $('.inputPercentualVariacao').hide();
  $('.inputValorVariacao').hide();
  $('.abrirEdicaoVariacao').show();
  $(this).hide();
});

//CLIQUE NO BOTÃO DE ABRIR EDIÇÃO DO SUBTOTAL
$('.abrirEdicaoFrete').on('click', function () {
  $(document).find('.negociacaoFreteValorVariacao').hide();
  $('.inputFreteVariacao').show();
  $(this).hide();
});

//CLIQUE NO BOTÃO DE ABRIR EDIÇÃO DA VARIAÇÃO
$('.abrirEdicaoVariacao').on('click', function () {
  $(document).find('.negociacaoPercentualVariacao').hide();
  $(document).find('.negociacaoValorVariacao').hide();
  $('.inputPercentualVariacao').show();
  $('.inputValorVariacao').show();
  $('.inputsNegociacaoVariacao').show();

  var valorInputVariacaoSubtotal = formatarFloat($('input[name="negociacaoSubtotal"]').val());
  $('.abrirEdicaoSubtotal').show();
  $('.inputSubtotalVariacao').hide();
  if(valorInputVariacaoSubtotal > 0){
    $(document).find('.negociacaoSubtotalValorVariacao').show();
    $(document).find('.negociacaoSubtotalVariacao').show();
  } else {
    $(document).find('.negociacaoSubtotalVariacao').hide();
  }

  $(this).hide();
});

/**
 * Função responsável por manipular os valores de subtotal
 */
function mudarValoresSubtotal(valorVariacao) {
  var elementoValorVariacao = $(document).find('.negociacaoSubtotalValorVariacao');
  $('input[name="negociacaoSubtotal"]').val(formatarPreco(valorVariacao, false));
  
  if (valorVariacao == 0) {
    $(document).find('.negociacaoSubtotalVariacao').hide();
    $(document).find('.abrirEdicaoSubtotal').show();
    $(document).find('.negociacaoSubtotalValorOriginal').css('font-weight', 'bold');
    return;
  }

  valorVariacao = formatarPreco(valorVariacao);

  elementoValorVariacao.text(valorVariacao);
  elementoValorVariacao.css('font-weight', 'bold');
  elementoValorVariacao.show();
  $(document).find('.negociacaoSubtotalValorOriginal').css('font-weight', 'lighter');
  $('.inputSubtotalVariacao').hide();

  $(document).find('.abrirEdicaoSubtotal').show();
}

/**
 * Função responsável por calcular a negociação pelo valor de variação
 */
function acionarVariacaoValor() {
  var valorVariacao      = formatarFloat($('input[name="negociacaoValorVariacao"]').val());
  var subtotalOriginal   = parseFloat(getValorOriginalSubtotal());

  var percentualVariacao          = (valorVariacao / subtotalOriginal) * 100;
  var percentualVariacaoFormatado = formatarPreco(percentualVariacao, false);

  $('input[name="negociacaoPercentualVariacao"]').val(percentualVariacaoFormatado);
  $('input[name="negociacaoPercentualVariacao"]').data('valor-original', percentualVariacao);
  acionarVariacaoPercentual();
}

/**
 * Função responsável por calcular a variação pelo percentual referente à negociação
 */
function acionarVariacaoPercentual() {
  var subtotalOriginal   = parseFloat(getValorOriginalSubtotal());
  var tipoVariacao       = getTipoVariacao();

  var percentualVariacao = parseFloat($('input[name="negociacaoPercentualVariacao"]').data('valor-original'));

  if((tipoVariacao == 'desconto') && percentualVariacao >= 100.00) percentualVariacao = 99;
  var subtotalVariacao   = 0;

  var novoSubtotal  = (subtotalOriginal * (100 - percentualVariacao)/100).toFixed(2);

  novoSubtotal = parseFloat(novoSubtotal);

  var valorVariacao = (Math.abs(subtotalOriginal - novoSubtotal)).toFixed(2);

  valorVariacao = parseFloat(valorVariacao);

  subtotalVariacao = (tipoVariacao == 'desconto') ? subtotalOriginal - valorVariacao : subtotalOriginal + valorVariacao;

  mudarValoresVariacao(percentualVariacao, valorVariacao);
  $('.abrirEdicaoSubtotal').click();
  mudarValoresSubtotal(subtotalVariacao);
}

/**
 * Função responsável por mudar os valores de variação (valor e percentual)
 */
function mudarValoresVariacao(percentualVariacao, valorVariacao) {
  elementoNegociacaoPercentualVariacao = $(document).find('.negociacaoPercentualVariacao');
  elementoNegociacaoValorVariacao = $(document).find('.negociacaoValorVariacao');

  elementoNegociacaoPercentualVariacao.text(formatarPreco(percentualVariacao, false) + ' %');
  elementoNegociacaoValorVariacao.text(formatarPreco(valorVariacao));

  $(document).find('.inputsNegociacaoVariacao').hide();

  $('input[name="negociacaoPercentualVariacao"]').val(formatarPreco(percentualVariacao, false));
  $('input[name="negociacaoPercentualVariacao"]').data('valor-original', percentualVariacao);
  $('input[name="negociacaoValorVariacao"]').val(formatarPreco(valorVariacao, false));
  
  $(document).find('.infosValoresVariacao').show();
  elementoNegociacaoValorVariacao.removeClass('hide');
  $('.abrirEdicaoVariacao').show();
}

/**
 * Função responsável por tratar os valores de variação da negociação
 */
function tratarValoresVariacao(valorSubtotalVariacao) {
  var subtotalOriginal            = parseFloat(getValorOriginalSubtotal());
  var tipoVariacao                = (valorSubtotalVariacao > subtotalOriginal) ? 'acrescimo' : 'desconto';
  var valorVariacao               = Math.abs(subtotalOriginal - valorSubtotalVariacao);
  var percentualVariacao          = (valorVariacao / subtotalOriginal) * 100;
  var percentualVariacaoFormatado = formatarPreco(percentualVariacao);

  if(tipoVariacao == 'desconto'){
    $('#tipoVariacaoDesconto').prop('checked', true);
    $('#tipoVariacaoAcrescimo').prop('checked', false);
  } else {
    $('#tipoVariacaoDesconto').prop('checked', false);
    $('#tipoVariacaoAcrescimo').prop('checked', true);
  }

  $('input[name="negociacaoPercentualVariacao"]').val(percentualVariacaoFormatado);
  $('input[name="negociacaoPercentualVariacao"]').data('valor-original', percentualVariacao);
  $('.abrirEdicaoVariacao').click();
  acionarVariacaoPercentual();
}

/**
 * Função responsável por abrir a popup de negociação
 */
function abrirNegociacao() {
  $(".abrePopupNegociacao").click();
}

/**
 * Função responsável por validar os valores de comissão no backend
 */
function salvarValoresComissao() {
  const boxComissao   = $(this).closest('.comissao');
  const nomeComissao  = boxComissao.find('.comissao-nome').text();
  const ajax          = boxComissao.attr('data-comissao') == 'reservaTecnica' ? 'rt' : 'ts';
  
  const preloader = new Preloader;
  preloader.show(`Calculando ${nomeComissao}...`);
  
  $.ajax({
    url: CAMINHO+`/checkout/ajax/comissao/ajax-checkout-comissao-salvar-valores-${ajax}.php`,
    data: {
      valorComissao: formatarFloat(boxComissao.find('.comissaoCampoValor').val()),
      valorPercentual: formatarFloat(boxComissao.find('.comissaoCampoPercentual').val()),
      campoAlterado: $(this).attr('data-tipo-campo')
    },
    type: 'POST',
		dataType: 'json',
    success: function(data) {
      boxComissao.find('.comissaoCampoValor').val(data.comissao);
      boxComissao.find('.comissaoCampoPercentual').val(data.percentual);

      boxComissao.find('.comissaoValor').text(data.comissaoFormatada);
      boxComissao.find('.comissaoPercentual').text(data.percentualFormatado);

      boxComissao.find('.comissaoInformativoVista-valorComissaoVista').text(data.comissaoVistaFormatado);
      boxComissao.find('.comissaoInformativoVista-porcentagemComissaoVista').text(data.percentualVistaFormatado);

      boxComissao.find('.comissaoInformativoPrazo-valorComissaoPrazo').text(data.comissaoFormatada);
      boxComissao.find('.comissaoInformativoPrazo-porcentagemComissaoPrazo').text(data.percentualFormatado);

      boxComissao.find('.comissao-frase').text(data.comissaoFrase);

      if(data.sucesso) {
        boxComissao.find('.erro').addClass('hide');

        // ESCONDE OS CAMPOS DE COMISSÃO
        boxComissao.find('.comissaoCampo').addClass('hide');

        // MOSTRA OS SPANS DE VALORES
        boxComissao.find('.comissaoValor, .comissaoPercentual, i').removeClass('hide');

        recarregarTela = true;

        preloader.hide();
        return;
      }

      boxComissao.find('.erro').removeClass('hide').text(data.erro);
      preloader.hide();
    }
  });
}

/**
 * Função responsável por salvar os dados de negociação de subtotal
 */
function salvarNegociacaoSubtotal() {
  var preloader = new Preloader;
  preloader.show('Aplicando negociação...');

	var url = CAMINHO + '/checkout/ajax/negociacao/ajax-checkout-negociacao-aplicar.php';

  jQuery.ajax({
		type: "POST",
		url: url,
		data: extrairDadosNegociacaoSubtotal(),
		dataType: "json"
  }).done(function(data) {
    recarregarTela = true;

    if(!data.comissoes) return;

    data.comissoes.forEach(comissao => {
      const boxComissao = $(`[data-comissao="${comissao.tipoComissao}"]`);

      boxComissao.find('.comissaoCampoValor').val(comissao.comissaoFinal);
      boxComissao.find('.comissaoCampoPercentual').val(comissao.percentualFinal);

      boxComissao.find('.comissaoValor').text(comissao.comissaoFinalFormatado);
      boxComissao.find('.comissaoPercentual').text(comissao.percentualFinalFormatado);

      boxComissao.find('.comissaoInformativoVista-valorComissaoVista').text(comissao.comissaoVistaFormatado);
      boxComissao.find('.comissaoInformativoVista-porcentagemComissaoVista').text(comissao.percentualVistaFormatado);

      boxComissao.find('.comissaoInformativoPrazo-valorComissaoPrazo').text(comissao.comissaoFormatada);
      boxComissao.find('.comissaoInformativoPrazo-porcentagemComissaoPrazo').text(comissao.percentualFormatado);

      boxComissao.find('.comissao-frase').text(comissao.comissaoFrase);      
    });  

    preloader.hide();
  }).fail(function(data){
    preloader.hide();
  });
}

/**
 * Função responsável por salvar os dados de negociação de subtotal
 */
function salvarNegociacaoFrete() {
  var preloader = new Preloader;
  preloader.show('Aplicando negociação...');

	var url = CAMINHO + '/checkout/ajax/negociacao/ajax-checkout-negociacao-aplicar.php';

  jQuery.ajax({
		type: "POST",
		url: url,
		data: extrairDadosNegociacaoFrete(),
		dataType: "json"
  }).done(function(data) {
    recarregarTela = true;

    $('#valorFreteOriginal').addClass('valorFreteOriginalNegociado').removeClass('valorFreteOriginal');
    let inputValorNegociacao   = parseFloat($('input[name="negociacaoFreteValor"]').val().replace('.', '').replace(',', '.'));
    let valorNegociacao        = !isNaN(inputValorNegociacao) ? inputValorNegociacao : '0.00';
        valorNegociacao        = formatarPreco(valorNegociacao);

    // MOSTRA OS INPUTS
    $('.inputNegociacaoFrete').addClass('hide');
  
    // ESCONDE OS SPANS DE VALORES
    $('.valorFreteNegociacao, .abrirEdicaoFrete').removeClass('hide').show();
    $('.valorFreteNegociacao').text(valorNegociacao);

    if(!data.comissoes) return;

    data.comissoes.forEach(comissao => {
      const boxComissao = $(`[data-comissao="${comissao.tipoComissao}"]`);

      boxComissao.find('.comissaoCampoValor').val(comissao.comissaoFinal);
      boxComissao.find('.comissaoCampoPercentual').val(comissao.percentualFinal);

      boxComissao.find('.comissaoValor').text(comissao.comissaoFinalFormatado);
      boxComissao.find('.comissaoPercentual').text(comissao.percentualFinalFormatado);

      boxComissao.find('.comissaoInformativoVista-valorComissaoVista').text(comissao.comissaoVistaFormatado);
      boxComissao.find('.comissaoInformativoVista-porcentagemComissaoVista').text(comissao.percentualVistaFormatado);

      boxComissao.find('.comissaoInformativoPrazo-valorComissaoPrazo').text(comissao.comissaoFormatada);
      boxComissao.find('.comissaoInformativoPrazo-porcentagemComissaoPrazo').text(comissao.percentualFormatado);

      boxComissao.find('.comissao-frase').text(comissao.comissaoFrase);      
    });

    preloader.hide();
  }).fail(function(data){
    preloader.hide();
  });
}

/**
 * Função responsável por extrair os dados de negociação do subtotal do formulário de negociação
 * @returns {Object}
 */
function extrairDadosNegociacaoSubtotal() {
  var tipoVariacao       = getTipoVariacao();
  var valorVariacao      = formatarFloat($('input[name="negociacaoValorVariacao"]').val());
  var percentualVariacao = parseFloat($('input[name="negociacaoPercentualVariacao"]').data('valor-original')).toFixed(13);
  var percentualOriginal = formatarFloat($('input[name="negociacaoPercentualVariacao"]').val());

  //SE NÃO VARIAR, O VALOR DA NEGOCIAÇÃO PERMANECE O MESMO DO SUBTOTAL ORIGINAL
  if(percentualVariacao == 0 || valorVariacao == 0) {
    $('input[name="negociacaoSubtotal"]').val(formatarPreco(getValorOriginalSubtotal(), false));
  }

  return {
    negociacao: 'subtotal',
    tipoVariacao: tipoVariacao,
    valorVariacao: valorVariacao,
    percentualVariacao: percentualVariacao,
    percentualOriginal: percentualOriginal,
    subtotalNegociacao: formatarFloat($('input[name="negociacaoSubtotal"]').val())
  };
}

/**
 * Função responsável por extrair os dados de negociação do subtotal do formulário de negociação
 * @returns {Object}
 */
function extrairDadosNegociacaoFrete() {
  var valorNegociacaoFrete = formatarFloat($('input[name="negociacaoFreteValor"]').val());
  var valorOriginalFrete   = formatarFloat($('#valorFreteOriginal').text());

  return {
    negociacao: 'frete',
    valorFreteNegociacao: valorNegociacaoFrete,
    valorFreteOriginal: valorOriginalFrete
  };
}

/**
 * Função responsável por efetuar o cancelamento da negociação
 */
function cancelarNegociacao() {
  const preloader = new Preloader;
  preloader.show('Cancelando negociação...');

  jQuery.ajax({
		type: "GET",
		url: CAMINHO + '/checkout/ajax/negociacao/ajax-checkout-negociacao-cancelar.php',
		dataType: "json"
  }).done(function() {
    parent.location.reload();
  }).fail(function(){
    parent.location.reload();
  });
}

/**
 * Função responsável por atualizar o valor original da porcentagem
 * @returns {void}
 */
function updateValorOriginalPorcentagem() {
  $('input[name="negociacaoPercentualVariacao"]').data('valor-original', formatarFloat($('input[name="negociacaoPercentualVariacao"]').val()));
}

//EVENTOS DE ALTERAÇÃO

/**
 * Ação de mudança do tipo de variação
 */
$('input[name="tipoVariacao"]').on('change', function(){
  acionarVariacaoPercentual();
  salvarNegociacaoSubtotal();
});

/**
 * Ação de mudança no input de percentual de variação da negociação
 */
$('input[name="negociacaoPercentualVariacao"]').on('change', function(){
  if($(this).val() == '') $(this).val(0);
  updateValorOriginalPorcentagem();
  acionarVariacaoPercentual();
  salvarNegociacaoSubtotal();
});

/**
 * Ação de apertar o ENTER para alterar o valor do percentual de variação
 */
$('input[name="negociacaoPercentualVariacao"]').on('keypress', function (e) {
  if (e.which == 13) {
    e.preventDefault();
    if($(this).val() == '') $(this).val(0);
    updateValorOriginalPorcentagem()
    acionarVariacaoPercentual();
    salvarNegociacaoSubtotal();
  }
});

/**
 * Ação mudança no input do valor da variação da negociação
 */
$('input[name="negociacaoValorVariacao"]').on('change', function(){
  if($(this).val() == '') $(this).val(0);
  acionarVariacaoValor();
  salvarNegociacaoSubtotal();
});

/**
 * Ação de apertar o ENTER para alterar o valor da variação
 */
$('input[name="negociacaoValorVariacao"]').on('keypress', function (e) {
  if (e.which == 13) {
    e.preventDefault();
    if($(this).val() == '') $(this).val(0);
    acionarVariacaoValor();
    salvarNegociacaoSubtotal();
  }
});

/**
 * Ação mudança no input do subotal da negociação
 */
$('input[name="negociacaoSubtotal"]').on('focusout', function () {
  if($(this).val() == '') $(this).val(0);
  
  var valorVariacao = formatarFloat($(this).val());

  mudarValoresSubtotal(valorVariacao);
  tratarValoresVariacao(valorVariacao);
  salvarNegociacaoSubtotal();
});

/**
 * Ação de apertar o ENTER para alterar o valor do subtotal
 */
$('input[name="negociacaoSubtotal"]').on('keypress', function (e) {
  if (e.which == 13) {
    e.preventDefault();

    var valorVariacao = formatarFloat($(this).val());

    mudarValoresSubtotal(valorVariacao);
    tratarValoresVariacao(valorVariacao);
    salvarNegociacaoSubtotal();
  }
});

/**
 * Ação responsável por mostrar os inputs de edição da comissão
 */
$(document).on('click', '.abrirEdicaoComissao', function() {
  const boxComissao = $(this).closest('.comissao');

  // MOSTRA OS INPUTS
  boxComissao.find('.comissaoCampo').removeClass('hide');

  // ESCONDE OS SPANS DE VALORES
  boxComissao.find('.comissaoValor, .comissaoPercentual, i').addClass('hide');
});

/**
 * Ação responsável por mostrar os inputs de edição da comissão
 */
$(document).on('click', '.abrirEdicaoFrete', function() {
  const boxComissao = $(this).closest('.negociacaoFrete');

  // MOSTRA OS INPUTS
  boxComissao.find('.inputNegociacaoFrete').removeClass('hide');

  // ESCONDE OS SPANS DE VALORES
  boxComissao.find('.valorFreteNegociacao, i').addClass('hide');
});

/**
 * Ação responsável por chamar a função que valida os valores de comissão no backend
 */
$(document).on('focusout', '.comissaoCampoValor, .comissaoCampoPercentual', salvarValoresComissao);

/**
 * Ação responsável por chamar a função que valida os valores de comissão no backend
 */
$(document).on('focusout', '.negociacaoFreteValor', salvarNegociacaoFrete);

/**
 * Ação responsável por fazer o submit do formulário de negociação
 */
$('.formularioNegociacao').on('submit', function(e) {
  e.preventDefault();

  recarregarTela ? parent.location.reload() : parent.$.fancybox.close();
});

$('.cancelarNegociacao').on('click', cancelarNegociacao);
// FUNÇÕES
function fecharPopupAgendamento() {
  parent.$.fancybox.close();
}

function salvarEntregaAgendada() {
  let formElement = $(this).closest('#form-entrega-agendada')[0];
  let formData    = new FormData(formElement);

  // PRELOADER
  let preloader = new Preloader();
  preloader.show('Cadastrando a data de entrega...');

  jQuery.ajax({
    url        : CAMINHO + '/checkout/ajax/entrega-agendada/ajax-checkout-entrega-agendada-salvar.php',
    type       : 'POST',
		dataType   : 'json',
		data       : formData,
		cache      : false,
		processData: false,
		contentType: false,
    success: data => {
      preloader.hide();
      let mensagem = data.mensagem;
      let alerta   = $('#alerta');
      
      // ADICIONA OS DADOS DO ALERTA
      alerta.find('.itemMensagem .mensagem').html(mensagem);
      switch(data.status) {
        case true:
          alerta.find('.alert').removeClass('alertDanger');
          alerta.find('.alert').addClass('alertSuccess');
          alerta.find('.itemMensagem .icone-alerta').html('done');
          break;
        
        default:
          alerta.find('.alert').removeClass('alertSuccess');
          alerta.find('.alert').addClass('alertDanger');
          alerta.find('.itemMensagem .icone-alerta').html('error');
          break;
      }

      // EXIBE O ALERTA
      alerta.find('.alert').attr('style', '');
      setTimeout(function() {
        alerta.find('.close').click();
      }, 5000);

      if(data.status) {
        fecharPopupAgendamento();

        if(formData.get('multiEstoque') == 'n') {
          let identificador        = 'input[value="'+formData.get('idFrete')+'"]';
          let opcaoEntregaAgendada = parent.$(identificador);
          
          // ADICIONA TRAVA PARA NÃO CHAMAR NOVAMENTE O AJAX PARA SALVAR OS DADOS DE AGENDAMENTO
          opcaoEntregaAgendada.attr('data-lock-entrega-agendada', true);
          
          opcaoEntregaAgendada.click();
        }
      }
    },
    error: erro => {
      console.error(erro);
      preloader.hide();
    }
  });
}
// FUNÇÕES

// CHAMADA DOS EVENTOS
// VERIFICA SE EXISTE A BOX DE AGENDAMENTO DE ENTREGA
if($('.container-entrega-agendada').length) {
  $(document).on('click', '[data-salvar-agendamento]', salvarEntregaAgendada);
}

// CHAMADA DOS EVENTOS
function copiarPix(){
  var copyText = document.getElementById("codigoPix");

  //ALTERA A VISIBILIDADE DO INPUT
  copyText.type = "text";

  copyText.select();
  copyText.setSelectionRange(0, 99999);
  document.execCommand("copy");
  $('.boxCheckout-codigoPixCopiado').slideDown(200,function(){
    setTimeout(function(){
      $('.boxCheckout-codigoPixCopiado').slideUp(200);
    },3000);
  });

  //RESTAURA A VISIBILIDADE DO INPUT
  copyText.type = "hidden";
}
