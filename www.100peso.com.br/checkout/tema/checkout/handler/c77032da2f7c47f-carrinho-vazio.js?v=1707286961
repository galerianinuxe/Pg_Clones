});

/**
* Metodo calcula o valor adicional da embalagem
* @param  string caminho Caminho da onde esta vindo a ação
* @return void
*/
function calcularPresente(caminho) {
	var idProduto         = caminho.attr('data-id');
	var hash              = caminho.attr('data-hash');
	var tipoProduto       = caminho.attr('data-tipo');
	var status            = caminho.is(':checked');
	var etapaCheckout     = null;

	if(typeof caminho.attr('data-etapa') != "undefined"){
		var etapaCheckout = caminho.attr('data-etapa');
		var preloader = new Preloader();
		preloader.show(status ? 'Calculando embalagem de presente...' : 'Removendo embalagem de presente...');
	}

	jQuery.ajax({
		type: "POST",
		data: { idProduto, hash, tipoProduto, status },
		url: CAMINHO + "/checkout/ajax/ajax-checkout-calcular-presente.php",
		dataType: "json",
		success: function success(data) {
			if(etapaCheckout == 'pagamento'){
				window.location = CAMINHO + '/checkout/pagamento';
			} else {
				$('.boxConcluir').html(data.totalCarrinho);
				//ATUALIZA OS DADOS DOS BRINDES
				funcBrindes();
				//ATUALIZA TOPO DA VENDA INTERNA
				atualizarTopoVendaInterna();
				//ATUALIZA MENSAGENS DO CARRINHO
				atualizarMensagensCarrinho();
				//VERIFICA SE POSSUI CUPOM ATIVO
				verificarCupomAtivo();
			}
		},
		error: function error(data) {
			console.log(data);
		}
	});
}

/**
* Metodo que atualiza as mensagens do carrinho
* @return void
*/
function atualizarMensagensCarrinho() {
	jQuery.ajax({
		type: "POST",
		url: CAMINHO + "/checkout/ajax/ajax-checkout-atualizar-mensagens-carrinho.php",
		dataType: "json",
		success: function success(data) {
			$('.boxMensagemPromocao').html(data.mensagemPromocao);
			$('.boxMensagemMinimoCompra').html(data.mensagemMinimoCompra);
			$('.boxMensagemLimiteCompra').html(data.mensagemLimiteCompra);
			$('.boxMensagemClienteInativo').html(data.mensagemClienteInativo);
		},
		error: function error(data) {
			console.log(data);
		}
	});
}


/** ADICIONAR MENSAGEM DE ERRO NO INPUT */
function adicionarMensagemErroInput(input,mensagem,focus = true){
  if(input.length){
		if(focus) input.focus();
		input.closest('.valida-input').removeClass('valido');
		input.closest('.valida-input').removeClass('alerta');
		input.closest('.valida-input').addClass('invalido').attr('data-mensagem',mensagem);
	}
}

/** ADICIONAR MENSAGEM DE SUCESSO NO INPUT */
function adicionarMensagemSucessoInput(input,mensagem){
	if(input.length){
		input.closest('.valida-input').removeClass('invalido');
		input.closest('.valida-input').removeClass('alerta');
		input.closest('.valida-input').addClass('valido').attr('data-mensagem',mensagem);
	}
}

/** ADICIONAR MENSAGEM DE ALERTA NO INPUT */
function adicionarMensagemAlertaInput(input,mensagem){
	if(input.length){
		input.closest('.valida-input').removeClass('valido');
		input.closest('.valida-input').removeClass('invalido');
		input.closest('.valida-input').addClass('alerta').attr('data-mensagem',mensagem);
	}
}

/** ADICIONAR MENSAGEM DE ERRO NO INPUT NO  CADASTRO*/
function adicionarMensagemErroInputCadastro(input,mensagem){
	if(input.length){
		boxMesagem = input.closest('.boxInputMensagemCheckoutCadastro');
    boxMesagem.addClass('mensagemAtiva');
    boxMesagem.find('.boxMensagemCheckoutCadastro').css('display','inline-block');
		boxMesagem.find('.mensagemCheckoutCadastro').html(mensagem);
	}
}

//REMOVE MENSAGENS DOS INPUTS VALIDATE
function removerMensagensValidate(inputValidate,removerTodosStatus = true){
	var status = (removerTodosStatus) ? 'statusValido statusInvalido' : 'statusInvalido';
	inputValidate.parent().find('.formError').remove();
	inputValidate.closest('.mainBox-conteudo-form-input-validacao').removeClass(status);
}

/** REMOVE MENSAGEM DO INPUT */
$(document).on('change keyup','.valida-input input',function(e){

	//TECLA PRESSIONADA
	var keyCode =  e.keyCode  ? e.keyCode  :
                 e.charCode ? e.charCode :
                 e.which    ? e.which    : void 0;

	//VERIFICA SE NÃO É O ENTER
  if(keyCode != 13){
    $(this).closest('.valida-input').removeClass('valido invalido alerta').attr('data-mensagem','');
  }
});

/** REMOVE MENSAGEM DO INPUT CADASTRO*/
$(document).on('change keyup','.boxInputMensagemCheckoutCadastro input',function(e){
	//TECLA PRESSIONADA
	var keyCode =  e.keyCode  ? e.keyCode  :
                 e.charCode ? e.charCode :
                 e.which    ? e.which    : void 0;

	//VERIFICA SE NÃO É O ENTER
  if(keyCode != 13){
    boxMesagem = $(this).closest('.boxInputMensagemCheckoutCadastro');
    boxMesagem.removeClass('mensagemAtiva');
    boxMesagem.find('.boxMensagemCheckoutCadastro').hide();
    boxMesagem.find('.mensagemCheckoutCadastro').html('');
  }
});

//VERIFICAR MENSAGEM DE ERRO NO CADASTRO
$(document).ready(function(){
	if($('#formCadastroUsuario').html()){
		var mensagemCadastro      = $('#formCadastroUsuario').data('mensagem');
		var campoMensagemCadastro = $('#formCadastroUsuario').data('campo-mensagem');
		if(mensagemCadastro.length && campoMensagemCadastro.length) {
			//ADICIONA MENSAGEM DE ERRO
			adicionarMensagemErroInput($('#formCadastroUsuario').find('input[name="'+campoMensagemCadastro+'"]'),mensagemCadastro);
		}
	}
});

var mostrarMensagemPendente = true;
//ATUALIZA CAMPO DE CONFIRMAÇÃO DE SENHA
$(document).on('submit','#formCadastroUsuario',function(){

	//VERIFICA MENSAGENS CADASTRO
	var boxMesagem = $(this).find('.boxInputMensagemCheckoutCadastro.mensagemAtiva, .valida-input.invalido');
	if(boxMesagem.html()){

		//ANCORA MENSAGEM
	  targetOffset = boxMesagem.offset().top;
	  $('html, body').animate({
	    scrollTop: targetOffset - 100
	  }, 500);

		//FOCUS NO INPUT
		boxMesagem.find('input').first().focus();

		return false;
	}

	//INFORMAÇÕES FISCAIS
	if(($('input[name="cpfCnpj"]').val().length <= 14) || ($('select[name="enquadramento"]').data('mostrar') == 'hide')){
		$('select[name="enquadramento"]').remove();
	}

	//CONFIRMAÇÃO SENHA
	$(this).find('input[name=confirmeSenha]').val($(this).find('input[name=senha]').val());

	var mensagemPendente = $(this).data('mensagem-pendente');

	if(mensagemPendente == "" || !mostrarMensagemPendente || $(this).data('tipo-formulario') == 'update') return true;

	var confirmCustomVars = {
    title: "Aviso",
    text: mensagemPendente,
    type: "warning",
    closeOnConfirm: true,
    html:true
  };
  retorno = function(e){
    if(e){
      mostrarMensagemPendente = false;
			$('#formCadastroUsuario').submit();
    }
  }
  confirmCustom(confirmCustomVars,retorno);

	return false;
});

