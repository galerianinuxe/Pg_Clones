/* jshint laxbreak: true */
/* jshint maxcomplexity:17 */
/* global define */

// UMD (Universal Module Definition) patterns for JavaScript modules that work everywhere.
// https://github.com/umdjs/umd/blob/master/templates/jqueryPlugin.js
(function (factory, jQuery, Zepto) {

    if (typeof define === 'function' && define.amd) {
        define(['jquery'], factory);
    } else if (typeof exports === 'object') {
        module.exports = factory(require('jquery'));
    } else {
        factory(jQuery || Zepto);
    }

}(function ($) {
    'use strict';

    var Maskcheckout = function (el, maskcheckout, options) {

        var p = {
            invalid: [],
            getCaret: function () {
                try {
                    var sel,
                        pos = 0,
                        ctrl = el.get(0),
                        dSel = document.selection,
                        cSelStart = ctrl.selectionStart;

                    // IE Support
                    if (dSel && navigator.appVersion.indexOf('MSIE 10') === -1) {
                        sel = dSel.createRange();
                        sel.moveStart('character', -p.val().length);
                        pos = sel.text.length;
                    }
                    // Firefox support
                    else if (cSelStart || cSelStart === '0') {
                        pos = cSelStart;
                    }

                    return pos;
                } catch (e) {}
            },
            setCaret: function(pos) {
                try {
                    if (el.is(':focus')) {
                        var range, ctrl = el.get(0);

                        // Firefox, WebKit, etc..
                        if (ctrl.setSelectionRange) {
                            ctrl.setSelectionRange(pos, pos);
                        } else { // IE
                            range = ctrl.createTextRange();
                            range.collapse(true);
                            range.moveEnd('character', pos);
                            range.moveStart('character', pos);
                            range.select();
                        }
                    }
                } catch (e) {}
            },
            events: function() {
                el
                .on('keydown.maskcheckout', function(e) {
                    el.data('maskcheckout-keycode', e.keyCode || e.which);
                    el.data('maskcheckout-previus-value', el.val());
                    el.data('maskcheckout-previus-caret-pos', p.getCaret());
                    p.maskcheckoutDigitPosMapOld = p.maskcheckoutDigitPosMap;
                })
                .on($.jMaskcheckoutGlobals.useInput ? 'input.maskcheckout' : 'keyup.maskcheckout', p.behaviour)
                .on('paste.maskcheckout drop.maskcheckout', function() {
                    setTimeout(function() {
                        el.keydown().keyup();
                    }, 100);
                })
                .on('change.maskcheckout', function(){
                    el.data('changed', true);
                })
                .on('blur.maskcheckout', function(){
                    if (oldValue !== p.val() && !el.data('changed')) {
                        el.trigger('change');
                    }
                    el.data('changed', false);
                })
                // it's very important that this callback remains in this position
                // otherwhise oldValue it's going to work buggy
                .on('blur.maskcheckout', function() {
                    oldValue = p.val();
                })
                // select all text on focus
                .on('focus.maskcheckout', function (e) {
                    if (options.selectOnFocus === true) {
                        $(e.target).select();
                    }
                })
                // clear the value if it not complete the maskcheckout
                .on('focusout.maskcheckout', function() {
                    if (options.clearIfNotMatch && !regexMaskcheckout.test(p.val())) {
                       p.val('');
                   }
                });
            },
            getRegexMaskcheckout: function() {
                var maskcheckoutChunks = [], translation, pattern, optional, recursive, oRecursive, r;

                for (var i = 0; i < maskcheckout.length; i++) {
                    translation = jMaskcheckout.translation[maskcheckout.charAt(i)];

                    if (translation) {

                        pattern = translation.pattern.toString().replace(/.{1}$|^.{1}/g, '');
                        optional = translation.optional;
                        recursive = translation.recursive;

                        if (recursive) {
                            maskcheckoutChunks.push(maskcheckout.charAt(i));
                            oRecursive = {digit: maskcheckout.charAt(i), pattern: pattern};
                        } else {
                            maskcheckoutChunks.push(!optional && !recursive ? pattern : (pattern + '?'));
                        }

                    } else {
                        maskcheckoutChunks.push(maskcheckout.charAt(i).replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&'));
                    }
                }

                r = maskcheckoutChunks.join('');

                if (oRecursive) {
                    r = r.replace(new RegExp('(' + oRecursive.digit + '(.*' + oRecursive.digit + ')?)'), '($1)?')
                         .replace(new RegExp(oRecursive.digit, 'g'), oRecursive.pattern);
                }

                return new RegExp(r);
            },
            destroyEvents: function() {
                el.off(['input', 'keydown', 'keyup', 'paste', 'drop', 'blur', 'focusout', ''].join('.maskcheckout '));
            },
            val: function(v) {
                var isInput = el.is('input'),
                    method = isInput ? 'val' : 'text',
                    r;

                if (arguments.length > 0) {
                    if (el[method]() !== v) {
                        el[method](v);
                    }
                    r = el;
                } else {
                    r = el[method]();
                }

                return r;
            },
            calculateCaretPosition: function() {
                var oldVal = el.data('maskcheckout-previus-value') || '',
                    newVal = p.getMaskcheckouted(),
                    caretPosNew = p.getCaret();
                if (oldVal !== newVal) {
                    var caretPosOld = el.data('maskcheckout-previus-caret-pos') || 0,
                        newValL = newVal.length,
                        oldValL = oldVal.length,
                        maskcheckoutDigitsBeforeCaret = 0,
                        maskcheckoutDigitsAfterCaret = 0,
                        maskcheckoutDigitsBeforeCaretAll = 0,
                        maskcheckoutDigitsBeforeCaretAllOld = 0,
                        i = 0;

                    for (i = caretPosNew; i < newValL; i++) {
                        if (!p.maskcheckoutDigitPosMap[i]) {
                            break;
                        }
                        maskcheckoutDigitsAfterCaret++;
                    }

                    for (i = caretPosNew - 1; i >= 0; i--) {
                        if (!p.maskcheckoutDigitPosMap[i]) {
                            break;
                        }
                        maskcheckoutDigitsBeforeCaret++;
                    }

                    for (i = caretPosNew - 1; i >= 0; i--) {
                        if (p.maskcheckoutDigitPosMap[i]) {
                            maskcheckoutDigitsBeforeCaretAll++;
                        }
                    }

                    for (i = caretPosOld - 1; i >= 0; i--) {
                        if (p.maskcheckoutDigitPosMapOld[i]) {
                            maskcheckoutDigitsBeforeCaretAllOld++;
                        }
                    }

                    // if the cursor is at the end keep it there
                    if (caretPosNew > oldValL) {
                      caretPosNew = newValL * 10;
                    } else if (caretPosOld >= caretPosNew && caretPosOld !== oldValL) {
                        if (!p.maskcheckoutDigitPosMapOld[caretPosNew])  {
                          var caretPos = caretPosNew;
                          caretPosNew -= maskcheckoutDigitsBeforeCaretAllOld - maskcheckoutDigitsBeforeCaretAll;
                          caretPosNew -= maskcheckoutDigitsBeforeCaret;
                          if (p.maskcheckoutDigitPosMap[caretPosNew])  {
                            caretPosNew = caretPos;
                          }
                        }
                    }
                    else if (caretPosNew > caretPosOld) {
                        caretPosNew += maskcheckoutDigitsBeforeCaretAll - maskcheckoutDigitsBeforeCaretAllOld;
                        caretPosNew += maskcheckoutDigitsAfterCaret;
                    }
                }
                return caretPosNew;
            },
            behaviour: function(e) {
                e = e || window.event;
                p.invalid = [];

                var keyCode = el.data('maskcheckout-keycode');

                if ($.inArray(keyCode, jMaskcheckout.byPassKeys) === -1) {
                    var newVal   = p.getMaskcheckouted(),
                        caretPos = p.getCaret();

                    // this is a compensation to devices/browsers that don't compensate
                    // caret positioning the right way
                    setTimeout(function() {
                      p.setCaret(p.calculateCaretPosition());
                    }, 10);

                    p.val(newVal);
                    p.setCaret(caretPos);
                    return p.callbacks(e);
                }
            },
            getMaskcheckouted: function(skipMaskcheckoutChars, val) {
                var buf = [],
                    value = val === undefined ? p.val() : val + '',
                    m = 0, maskcheckoutLen = maskcheckout.length,
                    v = 0, valLen = value.length,
                    offset = 1, addMethod = 'push',
                    resetPos = -1,
                    maskcheckoutDigitCount = 0,
                    maskcheckoutDigitPosArr = [],
                    lastMaskcheckoutChar,
                    check;

                if (options.reverse) {
                    addMethod = 'unshift';
                    offset = -1;
                    lastMaskcheckoutChar = 0;
                    m = maskcheckoutLen - 1;
                    v = valLen - 1;
                    check = function () {
                        return m > -1 && v > -1;
                    };
                } else {
                    lastMaskcheckoutChar = maskcheckoutLen - 1;
                    check = function () {
                        return m < maskcheckoutLen && v < valLen;
                    };
                }

                var lastUntranslatedMaskcheckoutChar;
                while (check()) {
                    var maskcheckoutDigit = maskcheckout.charAt(m),
                        valDigit = value.charAt(v),
                        translation = jMaskcheckout.translation[maskcheckoutDigit];

                    if (translation) {
                        if (valDigit.match(translation.pattern)) {
                            buf[addMethod](valDigit);
                             if (translation.recursive) {
                                if (resetPos === -1) {
                                    resetPos = m;
                                } else if (m === lastMaskcheckoutChar && m !== resetPos) {
                                    m = resetPos - offset;
                                }

                                if (lastMaskcheckoutChar === resetPos) {
                                    m -= offset;
                                }
                            }
                            m += offset;
                        } else if (valDigit === lastUntranslatedMaskcheckoutChar) {
                            // matched the last untranslated (raw) maskcheckout character that we encountered
                            // likely an insert offset the maskcheckout character from the last entry; fall
                            // through and only increment v
                            maskcheckoutDigitCount--;
                            lastUntranslatedMaskcheckoutChar = undefined;
                        } else if (translation.optional) {
                            m += offset;
                            v -= offset;
                        } else if (translation.fallback) {
                            buf[addMethod](translation.fallback);
                            m += offset;
                            v -= offset;
                        } else {
                          p.invalid.push({p: v, v: valDigit, e: translation.pattern});
                        }
                        v += offset;
                    } else {
                        if (!skipMaskcheckoutChars) {
                            buf[addMethod](maskcheckoutDigit);
                        }

                        if (valDigit === maskcheckoutDigit) {
                            maskcheckoutDigitPosArr.push(v);
                            v += offset;
                        } else {
                            lastUntranslatedMaskcheckoutChar = maskcheckoutDigit;
                            maskcheckoutDigitPosArr.push(v + maskcheckoutDigitCount);
                            maskcheckoutDigitCount++;
                        }

                        m += offset;
                    }
                }

                var lastMaskcheckoutCharDigit = maskcheckout.charAt(lastMaskcheckoutChar);
                if (maskcheckoutLen === valLen + 1 && !jMaskcheckout.translation[lastMaskcheckoutCharDigit]) {
                    buf.push(lastMaskcheckoutCharDigit);
                }

                var newVal = buf.join('');
                p.mapMaskcheckoutdigitPositions(newVal, maskcheckoutDigitPosArr, valLen);
                return newVal;
            },
            mapMaskcheckoutdigitPositions: function(newVal, maskcheckoutDigitPosArr, valLen) {
              var maskcheckoutDiff = options.reverse ? newVal.length - valLen : 0;
              p.maskcheckoutDigitPosMap = {};
              for (var i = 0; i < maskcheckoutDigitPosArr.length; i++) {
                p.maskcheckoutDigitPosMap[maskcheckoutDigitPosArr[i] + maskcheckoutDiff] = 1;
              }
            },
            callbacks: function (e) {
                var val = p.val(),
                    changed = val !== oldValue,
                    defaultArgs = [val, e, el, options],
                    callback = function(name, criteria, args) {
                        if (typeof options[name] === 'function' && criteria) {
                            options[name].apply(this, args);
                        }
                    };

                callback('onChange', changed === true, defaultArgs);
                callback('onKeyPress', changed === true, defaultArgs);
                callback('onComplete', val.length === maskcheckout.length, defaultArgs);
                callback('onInvalid', p.invalid.length > 0, [val, e, el, p.invalid, options]);
            }
        };

        el = $(el);
        var jMaskcheckout = this, oldValue = p.val(), regexMaskcheckout;

        maskcheckout = typeof maskcheckout === 'function' ? maskcheckout(p.val(), undefined, el,  options) : maskcheckout;

        // public methods
        jMaskcheckout.maskcheckout = maskcheckout;
        jMaskcheckout.options = options;
        jMaskcheckout.remove = function() {
            var caret = p.getCaret();
            p.destroyEvents();
            p.val(jMaskcheckout.getCleanVal());
            p.setCaret(caret);
            return el;
        };

        // get value without maskcheckout
        jMaskcheckout.getCleanVal = function() {
           return p.getMaskcheckouted(true);
        };

        // get maskcheckouted value without the value being in the input or element
        jMaskcheckout.getMaskcheckoutedVal = function(val) {
           return p.getMaskcheckouted(false, val);
        };

       jMaskcheckout.init = function(onlyMaskcheckout) {
            onlyMaskcheckout = onlyMaskcheckout || false;
            options = options || {};

            jMaskcheckout.clearIfNotMatch  = $.jMaskcheckoutGlobals.clearIfNotMatch;
            jMaskcheckout.byPassKeys       = $.jMaskcheckoutGlobals.byPassKeys;
            jMaskcheckout.translation      = $.extend({}, $.jMaskcheckoutGlobals.translation, options.translation);

            jMaskcheckout = $.extend(true, {}, jMaskcheckout, options);

            regexMaskcheckout = p.getRegexMaskcheckout();

            if (onlyMaskcheckout) {
                p.events();
                p.val(p.getMaskcheckouted());
            } else {
                if (options.placeholder) {
                    el.attr('placeholder' , options.placeholder);
                }

                // this is necessary, otherwise if the user submit the form
                // and then press the "back" button, the autocomplete will erase
                // the data. Works fine on IE9+, FF, Opera, Safari.
                if (el.data('maskcheckout')) {
                  el.attr('autocomplete', 'off');
                }

                // detect if is necessary let the user type freely.
                // for is a lot faster than forEach.
                for (var i = 0, maxlength = true; i < maskcheckout.length; i++) {
                    var translation = jMaskcheckout.translation[maskcheckout.charAt(i)];
                    if (translation && translation.recursive) {
                        maxlength = false;
                        break;
                    }
                }

                if (maxlength) {
                    el.attr('maxlength', maskcheckout.length);
                }

                p.destroyEvents();
                p.events();

                var caret = p.getCaret();
                p.val(p.getMaskcheckouted());
                p.setCaret(caret);
            }
        };

        jMaskcheckout.init(!el.is('input'));
    };

    $.maskcheckoutWatchers = {};
    var HTMLAttributes = function () {
        var input = $(this),
            options = {},
            prefix = 'data-maskcheckout-',
            maskcheckout = input.attr('data-maskcheckout');

        if (input.attr(prefix + 'reverse')) {
            options.reverse = true;
        }

        if (input.attr(prefix + 'clearifnotmatch')) {
            options.clearIfNotMatch = true;
        }

        if (input.attr(prefix + 'selectonfocus') === 'true') {
           options.selectOnFocus = true;
        }

        if (notSameMaskcheckoutObject(input, maskcheckout, options)) {
            return input.data('maskcheckout', new Maskcheckout(this, maskcheckout, options));
        }
    },
    notSameMaskcheckoutObject = function(field, maskcheckout, options) {
        options = options || {};
        var maskcheckoutObject = $(field).data('maskcheckout'),
            stringify = JSON.stringify,
            value = $(field).val() || $(field).text();
        try {
            if (typeof maskcheckout === 'function') {
                maskcheckout = maskcheckout(value);
            }
            return typeof maskcheckoutObject !== 'object' || stringify(maskcheckoutObject.options) !== stringify(options) || maskcheckoutObject.maskcheckout !== maskcheckout;
        } catch (e) {}
    },
    eventSupported = function(eventName) {
        var el = document.createElement('div'), isSupported;

        eventName = 'on' + eventName;
        isSupported = (eventName in el);

        if ( !isSupported ) {
            el.setAttribute(eventName, 'return;');
            isSupported = typeof el[eventName] === 'function';
        }
        el = null;

        return isSupported;
    };

    $.fn.maskcheckout = function(maskcheckout, options) {
        options = options || {};
        var selector = this.selector,
            globals = $.jMaskcheckoutGlobals,
            interval = globals.watchInterval,
            watchInputs = options.watchInputs || globals.watchInputs,
            maskcheckoutFunction = function() {
                if (notSameMaskcheckoutObject(this, maskcheckout, options)) {
                    return $(this).data('maskcheckout', new Maskcheckout(this, maskcheckout, options));
                }
            };

        $(this).each(maskcheckoutFunction);

        if (selector && selector !== '' && watchInputs) {
            clearInterval($.maskcheckoutWatchers[selector]);
            $.maskcheckoutWatchers[selector] = setInterval(function(){
                $(document).find(selector).each(maskcheckoutFunction);
            }, interval);
        }
        return this;
    };

    $.fn.maskcheckouted = function(val) {
        return this.data('maskcheckout').getMaskcheckoutedVal(val);
    };

    $.fn.unmaskcheckout = function() {
        clearInterval($.maskcheckoutWatchers[this.selector]);
        delete $.maskcheckoutWatchers[this.selector];
        return this.each(function() {
            var dataMaskcheckout = $(this).data('maskcheckout');
            if (dataMaskcheckout) {
                dataMaskcheckout.remove().removeData('maskcheckout');
            }
        });
    };

    $.fn.cleanVal = function() {
        return this.data('maskcheckout').getCleanVal();
    };

    $.applyDataMaskcheckout = function(selector) {
        selector = selector || $.jMaskcheckoutGlobals.maskcheckoutElements;
        var $selector = (selector instanceof $) ? selector : $(selector);
        $selector.filter($.jMaskcheckoutGlobals.dataMaskcheckoutAttr).each(HTMLAttributes);
    };

    var globals = {
        maskcheckoutElements: 'input,td,span,div',
        dataMaskcheckoutAttr: '*[data-maskcheckout]',
        dataMaskcheckout: true,
        watchInterval: 300,
        watchInputs: true,
        // old versions of chrome dont work great with input event
        useInput: !/Chrome\/[2-4][0-9]|SamsungBrowser/.test(window.navigator.userAgent) && eventSupported('input'),
        watchDataMaskcheckout: false,
        byPassKeys: [9, 16, 17, 18, 36, 37, 38, 39, 40, 91],
        translation: {
            '0': {pattern: /\d/},
            '9': {pattern: /\d/, optional: true},
            '#': {pattern: /\d/, recursive: true},
            'A': {pattern: /[a-zA-Z0-9]/},
            'S': {pattern: /[a-zA-Z]/}
        }
    };

    $.jMaskcheckoutGlobals = $.jMaskcheckoutGlobals || {};
    globals = $.jMaskcheckoutGlobals = $.extend(true, {}, globals, $.jMaskcheckoutGlobals);

    // looking for inputs with data-maskcheckout attribute
    if (globals.dataMaskcheckout) {
        $.applyDataMaskcheckout();
    }

    setInterval(function() {
        if ($.jMaskcheckoutGlobals.watchDataMaskcheckout) {
            $.applyDataMaskcheckout();
        }
    }, globals.watchInterval);
}, window.jQuery, window.Zepto));
