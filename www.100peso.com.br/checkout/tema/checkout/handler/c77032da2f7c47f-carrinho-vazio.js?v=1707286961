* Atualiza as abas do formulario do checkout
*/
function refreshTabsFoundation() {
	Foundation.reInit($('[data-tabs]'));
}

/* VERIFICA SE O NOME ESTÁ COMPLETO */
function checkNome(field, rules, i, options) {
		mensagem   = 'Digite o nome completo';
	var labelRazao = $('input[name="nomeRazao"]').closest('.boxCampoCheckoutCadastro').find('.labelCadastro').html();

	if (labelRazao != undefined && labelRazao != 'Nome completo *') {
		mensagem = 'Digite a razão social completa';
	}
	valor = /^(([a-z&]|[À-ÿ]|[0-9.])+\s)+[^\t\n]+/i.test(field.val());
	if (!valor) return mensagem;
}

/* VERIFICA SE O NOME ESTÁ COMPLETO */
function checkNomeCompleto(field, rules, i, options) {
	valor = /^(([a-z&]|[À-ÿ]|[0-9.])+\s)+[^\t\n]+/i.test(field.val());

	if(!valor) return 'Digite o nome completo';
}

