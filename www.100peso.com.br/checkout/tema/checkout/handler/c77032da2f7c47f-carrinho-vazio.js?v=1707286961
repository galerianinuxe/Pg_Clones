$('.mascaraCEP').maskcheckout('00000-000');
//MASCARA DE CELULAR
$('.mascaraCelular').maskcheckout('(00) 00000-0000');
//MASCARA DE TELEFONE
$('.mascaraTelefone').maskcheckout('(00) 0000-0000');
//MASCARA DE DATA
$(".maskCheckoutData").maskcheckout('00/00/0000', { clearIfNotMatch: true });

//Mascara Celular
var SPMaskBehaviorCelular = function SPMaskBehaviorCelular(val) {
	return val.replace(/\D/g, '').length === 11 ? '(00) 00000-0000' : '(00) 00000-0000';
},
    spOptionsCelular = {
	clearIfNotMatch: true,
	onKeyPress: function onKeyPress(val, e, field, options) {
		field.maskcheckout(SPMaskBehaviorCelular.apply({}, arguments), options);
	}
};
$('.maskCheckoutTelefone.celular').maskcheckout(SPMaskBehaviorCelular, spOptionsCelular);

//Mascara telefone fixo
var SPMaskBehaviorTelefone = function SPMaskBehaviorTelefone(val) {
	return val.replace(/\D/g, '').length === 11 ? '(00) 00000-0000' : '(00) 0000-00009';
},
    spOptionsTelefone = {
	clearIfNotMatch: true,
	onKeyPress: function onKeyPress(val, e, field, options) {
		field.maskcheckout(SPMaskBehaviorTelefone.apply({}, arguments), options);
	}
};
$('.maskCheckoutTelefone.telefone').maskcheckout(SPMaskBehaviorTelefone, spOptionsTelefone);

/**
* Manipula o form de acordo com o tipo de pessoa no cadastro
* @method mudarParametrosCadastroCliente
* @param  {String} tipoCliente Tipo do cliente
*/
function mudarParametrosCadastroCliente() {
	var tipoCliente = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 'fisica';

	if (tipoCliente == 'juridica') {
		// $('input[name="cpfCnpj"]').closest('.boxCampoCheckoutCadastro').find('.labelCadastro').html('CNPJ');
		$('input[name="nomeRazao"]').closest('.boxCampoCheckoutCadastro').find('.labelCadastro').html('Razão Social *');

		//ENQUADRAMENTO FISCAL DA SESSÃO
		let enquadramentoSessao = $('select[name="enquadramento').data('enquadramento') ? $('select[name="enquadramento').data('enquadramento') : 0;

		$('select[name="tributacao]').removeProp('selected');;
		$('select[name="tributacao"] option[value="c"]').prop('selected','selected');
		$('input[name="rgIe"]').addClass('validate[required]');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').find('.labelCadastro').text('Inscrição Estadual *');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').css('display','block');
		$('select[name="enquadramento').removeProp('selected');
		$('select[name="enquadramento"] option[value="'+enquadramentoSessao+'"]').prop('selected','selected');
		$('input[name="rgIe"]').closest('.boxCampoCheckoutCadastro').show();
		$('input[name="nomeFantasia"]').closest('.boxCampoCheckoutCadastro').show();
		if($('select[name="enquadramento"]').data('mostrar') == 'show') $('select[name="enquadramento"]').closest('.boxCampoCheckoutCadastro').show();

		$('input[name="nascimento"]').closest('.boxCampoCheckoutCadastro').hide();
		$('input[name="sexo"]').closest('.boxCampoCheckoutCadastro').hide();
	} else {
		$('input[name="rgIe"]').prop('readonly', false);
		$('select[name="tributacao"]').removeProp('selected');;
		$('select[name="tributacao"] option[value="n"]').prop('selected','selected');
		$('select[name="enquadramento"]').removeProp('selected');;
		$('select[name="enquadramento"] option[value="-1"]').prop('selected','selected');

		// $('input[name="cpfCnpj"]').closest('.boxCampoCheckoutCadastro').find('.labelCadastro').html('CPF');
		$('input[name="nomeRazao"]').closest('.boxCampoCheckoutCadastro').find('.labelCadastro').html('Nome completo *');

		$('input[name="rgIe"]').closest('.boxCampoCheckoutCadastro').hide();
		$('input[name="nomeFantasia"]').closest('.boxCampoCheckoutCadastro').hide();
		$('select[name="enquadramento"]').closest('.boxCampoCheckoutCadastro').hide();

		$('input[name="nascimento"]').closest('.boxCampoCheckoutCadastro').show();
		$('input[name="sexo"]').closest('.boxCampoCheckoutCadastro').show();
	}
}

/**
 * Select de tributação do cadastro do cliente
 */
$(document).on('change','select[name="tributacao"]',function(){
	let opcaoTributacao = $(this).find(':selected').val();

	var campoInscricaoEstadual = $('input[name="rgIe"]');
	//LIMPA O CAMPO DE INSCRIÇÃO ESTADUAL
	removerMensagensValidate($('input[name="rgIe"]'));
	campoInscricaoEstadual.val('');


	if(opcaoTributacao == 'i'){
		campoInscricaoEstadual.prop('readonly', true);
    	campoInscricaoEstadual.removeClass('validate[required]');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').css('display','none');
	}
	if(opcaoTributacao == 'c'){
		campoInscricaoEstadual.prop('readonly', false);
		campoInscricaoEstadual.addClass('validate[required]');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').find('.labelCadastro').text('Inscrição Estadual *');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').css('display','block');
	}
	if(opcaoTributacao == 'n'){
		campoInscricaoEstadual.prop('readonly', false);
		campoInscricaoEstadual.removeClass('validate[required]');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').find('.labelCadastro').text('Inscrição Estadual');
		$('.boxCampoCheckoutCadastroInscricaoEstadual').css('display','block');
	}
});
/******************************************* CADASTRO ENDEREÇO ***************************************/

/**
* Clique do campo do cep do formulario
*/
$(document).on('keyup change', '#cepAutoComplete', function () {
	$(this).parent().find('.formError').remove();

	var cep = $(this).val();
	cep = cep.replace('-', '');

	if (!isNaN(cep) && cep.length == 8) {
		autoCompleteCep(cep);
	} else {
		situacaoFormularioEndereco('cadastro-cep');
	}
});

$(document).on('keypress', '#cepAutoComplete', function (e) {
	if (e.which == 13) {
		$('#cepAutoComplete').trigger('change');
		return false;
	}
});

/**
* Função busca o cep e monta em tela
*/
function autoCompleteCep(cep) {
	var url = CAMINHO + '/checkout/ajax/ajax-cep-auto-complete.php';
	jQuery.ajax({
		type: "POST",
		url: url,
		dataType: "json",
		data: {
			'cep': cep
		},
		success: function success(data) {
			$('.errorCEP').hide();
			if (data.status == 'sucesso') {
				$('#previewEndereco').html(data.preview);
				$('input[name="endereco"]').val(data.endereco);
				$('input[name="bairro"]').val(data.bairro);
				$('input[name="cidade"]').val(data.cidade);
				$('input[name="uf"]').val(data.estado);
				situacaoFormularioEndereco('cadastro-simples');
			} else if (data.status == 'cadastro') {
				$('input[name="endereco"]').val('');
				$('input[name="bairro"]').val('');
				$('input[name="cidade"]').val(data.cidade);
				$('input[name="uf"]').val(data.estado);
				situacaoFormularioEndereco('cadastro-completo');
			} else {
				$('.errorCEP').html(data.mensagem);
				$('.errorCEP').show();
				//ROLA A TELA PARA O TOPO DO MENSAGEM
				$('html, body').animate({
					scrollTop: $('.topoFormularioCheckoutEntrega').offset().top
				}, 500);
			}
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);
		}
	});
}

/**
 * Função responsável por alterar o tipo do formulário de cadastro de endereço
 * @param string			tipo			Tipo do formulário
 */
function setTipoFormularioEndereco(tipo) {
	document.getElementById('tipoFormularioEndereco').value = tipo;
}

/**
 * Função aciona os estagios do formulario de cadastro de endereco
 * @method situacaoFormularioEndereco
 * @param  string   situacao Estagio que o formulario esta sendo requisitado
 */
function situacaoFormularioEndereco(situacao) {
	if (situacao == 'cadastro-simples') {
		setTipoFormularioEndereco('automatico');
		$('.boxCheckout-newAdress').slideDown(300);
		$('.boxCheckout-newAdressCadastro').slideUp(300);
		$('input[name="numeroRapido"]').focus();
	} else if (situacao == 'cadastro-completo') {
		setTipoFormularioEndereco('manual');
		$('.boxCheckout-newAdressCadastro').show(300);
		$('.boxCheckout-newAdress').slideUp(300);
		$('input[name="nome"]').focus();
	} else if (situacao == 'cadastro-cep') {
		setTipoFormularioEndereco('automatico');
		$('.boxCheckout-newAdressCadastro').slideUp(300);
		$('.boxCheckout-newAdress').slideUp(300);
	}

	//VALIDA CAMPOS PREENCHIDOS
	validarCamposPreenchidos();
}

/**
* Clique abre o formulario de cadastro endereço
*/
$(document).on('click', '.open-adressClosed', function () {
	$('.boxCheckout-adressClosed').slideDown(300);
	$('.boxCheckout-selecionarFrete').slideUp(300);
	$('.boxCheckout-radioHabilitado').addClass('boxCheckout-radioDesabilitado');
	$(this).slideUp(300);

	var cep = $('input[name="cep"]').val();
	cep = cep.replace('-', '');
	if (!isNaN(cep) && cep.length == 8) {
		autoCompleteCep(cep);
	} else {
		$('.focus-CEP').focus();
	}

	//VALIDA CAMPOS PREENCHIDOS
	// validarCamposPreenchidos();
});

$(document).on('click', '.boxCheckout-cancelaNewAdress, .boxCheckout-radioHabilitado', function () {
	$('.boxCheckout-adressClosed').slideUp(300);
	$('.boxCheckout-selecionarFrete').slideDown(300);
	$('.boxCheckout-radioHabilitado').removeClass('boxCheckout-radioDesabilitado');

	$('.open-adressClosed').slideDown(300);
	$('.boxCheckout-newAdress').slideUp(300);
	$('.boxCheckout-newAdressCadastro').slideUp(300);
});

$(document).on('click', '.boxCheckout-wrongAdress', function () {
	// ALTERA O TIPO DO FORMULÁRIO
	setTipoFormularioEndereco('manual');

	situacaoFormularioEndereco('cadastro-completo');
});

/**
* Blur do campo de cadastro rapido
*/
$(document).on('blur', 'input[name="numeroRapido"], input[name="complementoRapido"], input[name="nomeRapido"], input[name="destinatarioRapido"]', function () {
	espelharDadosEndereco($(this));
});

/**
* Função espelha os dados do Cadastro Rapido para o completo e vice e versa
* @method espelharDadosEndereco
*/
function espelharDadosEndereco(caminho) {
	var valor = caminho.val();
	var nomeInput = caminho.attr('name');
	nomeInput = nomeInput.replace('Rapido', '');

	$('input[name="' + nomeInput + '"]').val(valor);

	//ATUALIZA OS LABELS DO CADASTRO
	// atualizarLabels();

	//VALIDA CAMPOS PREENCHIDOS
	// validarCamposPreenchidos();
}

/**
* Se tiver um CEP em sessao que não seja listado ele abre o formulario para o cadastro
*/
$(document).ready(function () {
	if (typeof cadastrarCepSessao != "undefined") {
		ativarCadastroCepSessao();
	} else if ($('input[name="enderecoSelecionado"]:checked').length &&  !$('.alterarPraca').html()) {
		let prazoMaximo        = $(this).data('selecionar-entrega-agendada');
		let mostrarAgendamento = prazoMaximo !== undefined;
		if(!mostrarAgendamento) prazoMaximo = 0;

		calcularFreteEndereco('endereco', mostrarAgendamento, true, prazoMaximo, this);
	}

	function ativarCadastroCepSessao() {
		$('.open-adressClosed').click();
	}
});

/**
* Opcao de enter do desconto cep
*/
$(document).on('keypress', 'input[name="cep"]', function (e) {
	if (e.which == 13) {
		$('button[name="btnCalcularFrete"]').click();
	}
});

/**
* Click do botao do calculo do FRETE e OPÇÃO Frete do carrinho
*/
$(document).on('click', 'button[name="btnCalcularFrete"], input[name="tipoFrete"]', function (e) {
	let prazoMaximo        = $(this).data('selecionar-entrega-agendada');
	let mostrarAgendamento = prazoMaximo !== undefined;
	if(!mostrarAgendamento) prazoMaximo = 0;

	calcularFreteEndereco('carrinho', mostrarAgendamento, true, prazoMaximo, this);
});

/**
* Clique do botao para alternar o CEP de destino
*/
$(document).on('click', 'input[name="enderecoSelecionado"], input[name="envio"]', function () {
	let prazoMaximo        = $(this).data('selecionar-entrega-agendada');
	let mostrarAgendamento = prazoMaximo !== undefined;
	if(!mostrarAgendamento) prazoMaximo = 0;

	calcularFreteEndereco('endereco', mostrarAgendamento, true, prazoMaximo, this);
});

/**
 * Função para calcular o frete no endereço
 * @param  string       sessao      Onde o cálculo de frete está sendo chamado
 * @param  bool         agendamento Define se a opção de frete é do tipo de entrega agendada
 * @param  bool         popup       Define se já foi chamada a popup de agendamento
 * @param  bool         prazoMaximo Define a data máxima para a entrega
 * @param  ELementDOM   element     Elemento onde a ação foi disparada
 * @return layout tipos de frete
 */
function calcularFreteEndereco(sessao, agendamento, popup, prazoMaximo, element = null) {
	if(sessao === undefined) sessao = 'carrinho';
	if(agendamento === undefined) agendamento = false;
	if(popup === undefined) popup = false;

	var idCepSelecionado = null;
	var idFrete          = null;
	var cep              = null;

	if (sessao == 'carrinho') {
		$('input[name="cep"]').closest('.valida-input').removeClass('valido invalido alerta').attr('data-mensagem','');
		//MONTAR VARIAVEIS PARA O CARRINHO
		var cepSelecionado = $('input[name="tipoFrete"]:checked');
		var retirada = cepSelecionado.data('retirada') ?? null;
		var pontoRetirada = cepSelecionado.data('ponto-retirada') ?? null;
		cep = $('input[name="cep"]').val().length ? $('input[name="cep"]').val() : '';
		idFrete = cepSelecionado.length ? cepSelecionado.val() : 0;
		var idEndereco = '';
		var prioridade = cepSelecionado.data('multi-estoque-prioridade');
	} else {
		//MONTAR VARIAVEIS PARA O ENDERECO
		var cepSelecionado = $('input[name="enderecoSelecionado"]:checked');
		var freteSelecionado = $('input[name="envio"]:checked');
		var retirada = cepSelecionado.data('tipo') == 'retirada' ? 's' : 'n';
		var pontoRetirada = 'n';
		idCepSelecionado = cepSelecionado.val();
		idFrete = freteSelecionado.val();
		cep = cepSelecionado.data('cep') ?? '';
		var idEndereco = $("[name='enderecoSelecionado']:checked").val();
		var prioridade = freteSelecionado.data('multi-estoque-prioridade');
		//VERIFICA PONTO DE RETIRADA
		if (retirada == 's' && freteSelecionado.data('ponto-retirada') == 's') {
			retirada = 'n';
			pontoRetirada = 's';
		}
		//VERIFICA TIPO FRETE
		if (retirada == 'n' && pontoRetirada == 'n' && idFrete && idFrete.substring(0, 9) == 'retirada-') {
			idFrete = null;
		}
	}

	if(agendamento && popup) {
		if(prazoMaximo === undefined) prazoMaximo = 0;

		$('.abrirEntregaAgendada').attr('data-id-frete', idFrete);
		$('.abrirEntregaAgendada').attr('data-prazo-maximo', prazoMaximo);
		
		if($(element).attr('data-lock-entrega-agendada') === undefined) {
			let multiEstoque = $(this).closest('div').find('input').attr('data-multi-estoque') !== undefined;
			getPopupEntregaAgendada(sessao, multiEstoque, prazoMaximo);
			return;
		}
	}

	var preloader = new Preloader();
	preloader.show('Calculando Frete...');

	var url = CAMINHO + '/checkout/ajax/ajax-checkout-carrinho-calcular-frete-checkout.php';
	jQuery.ajax({
		type: "POST",
		url: url,
		data: {
			cep: cep,
			idCepSelecionado: idCepSelecionado,
			idFrete: idFrete,
			sessao: sessao,
			idEndereco: idEndereco,
			retirada: retirada,
			pontoRetirada: pontoRetirada,
			prioridade: prioridade,
			agendamento: agendamento
		},
		dataType: "json",
		success: function success(data) {
			//VERIFICA ALTERAÇÕES NOS ESTOQUES DOS ITENS
			if(data.statusAlteracaoEstoque){
				window.location = CAMINHO+'/checkout/carrinho/?acao='+data.hashAcaoEstoque;
			}else{
				preloader.hide();

				//VERIFICA ALTERAÇÃO NA DISPONIBILIDADE DAS PICKUPS
        		var alteracaoDisponibilidadePickup = false;

				//ATUALIZA TOPO VENDA INTERNA
				atualizarTopoVendaInterna();
				//ATUALIZA MENSAGENS DO CARRINHO
				atualizarMensagensCarrinho();
				if (sessao == 'carrinho') {
					//MONTAR RETORNO NA TELA DO CARRINHO
					if (data.erro !== null && data.erro.length) {
						adicionarMensagemErroInput($('input[name="cep"]'),data.erro);
						$('.box-oculto-cep-obs').hide();
					}else if (data.opcoesFreteSimples !== null && data.opcoesFreteSimples.length) {
						$('.box-oculto-cep-obs').show();
					}
					if (data.produtos !== null) {
						$('.produtos').html(data.produtos);
						$('#carrinho-brindes').html(data.alertasBrindes);
						refreshTHB();
						funcBrindes();
					}
					if (data.totalCarrinho !== null) {
						$('.boxConcluir').html(data.totalCarrinho);
						//ATUALIZA OS DADOS DOS BRINDES
						funcBrindes();
						ajustarLayoutPersonalizacao();
					}
					if (data.opcoesFreteSimples !== null) {
						$('.boxCarrinhoFrete .boxCarrinhoFreteOpcoes').html(data.opcoesFreteSimples);
						verificarPickupSelecionado();
					}
					if (data.boxFreteGratis !== null) {
						$('.boxProdutoCarrinho .boxCarrinhoFreteGratis').html(data.boxFreteGratis);
					}

					if (data.opcaoFreteMultiEstoque !== null) {
						$('.produtos').html(data.opcaoFreteMultiEstoque);
						if(data.opcoesFreteSimples === null) {
						  $('.boxCarrinhoFrete .boxCarrinhoFreteOpcoes').html('');
						  $('.box-oculto-cep-obs').hide();
						}
						refreshTHB();
						funcBrindes();
						ajustarLayoutPersonalizacao();
					}
				} else {
					//MONTAR RETORNO NA TELA DO ENDERECO
					if (data.opcoesFreteSimples !== null) {
						$('.boxCheckout-selecionarFrete').html(data.opcoesFreteSimples);
						gtmVerificarFreteSelecionado();
					}
					// MONTANDO O RETORNO DOS PICKUPS
					var statusPickup = (data.pickups !== null && data.pickups != '');
					if($('#transp').length){
						if(statusPickup) {
							$('#transp').parent().find('.boxFretesItens').html(data.pickups);
						}else{
							alteracaoDisponibilidadePickup = true;
						}
						verificarPickupSelecionadoEndereco();
					}else {
						if(statusPickup) alteracaoDisponibilidadePickup = true;
					}
					if (data.opcaoFreteMultiEstoque !== null) {
						// $('.boxCheckout-selecionarFrete a').css('display', 'block');
						$('.boxCheckout-selecionarFrete .botaoPagamentoMultiEstoque').show();
						$('.produtos').html(data.opcaoFreteMultiEstoque);
						if(data.opcoesFreteSimples === null) {
						  $('.mainBox-conteudo-form-cadastro-entrega-cep-entrega-opcoes').hide();
						}
						refreshTHB();
					}
					if (data.resumoProdutos !== null) {
						$('.produtos').html(data.resumoProdutos);
						refreshTHB();
					}
					if (data.totalCarrinho !== null) {
						$('.boxResumoTotalizacao').html(data.totalCarrinho);
					}
					if(data.entregaAlteradaMultiEstoque || (alteracaoDisponibilidadePickup && statusPickup)){
					  var alertCustomVars = {
							title:"Alteração de entrega",
							text:"As informações de entrega foram atualizadas!",
							type:"success",
							confirmButtonText:"Ok",
							allowEscapeKey:true,
							html:true
						};
						alertCustom(alertCustomVars);
					}else if(alteracaoDisponibilidadePickup && !statusPickup){
						$('#transp').closest('.escolhaRecebimento-containerOpcs-item').remove();
					}

					// ALTERA O ENDEREÇO DE FATURAMENTO
					if(data.alterarEnderecoFaturamento) {
						document.querySelectorAll('[name="endereco-faturamento"]').forEach( opcao => {
							if(opcao.checked) opcao.checked = false;
													
							if(parseInt(opcao.value) == idCepSelecionado) opcao.checked = true;
						});
					}
				}
				// ADICIONA MENSAGEM DE RESTRIÇÃO DE ENTREGA PARA O CEP
				if(data.labelRestricaoEntrega !== null && (['carrinho','endereco'].indexOf(sessao) >= 0)){
					var boxSelecaoTipoFrete = $('.boxCheckoutSelecaoFretes,.boxCarrinhoFrete').find('.boxRestricaoEntrega');
					if(boxSelecaoTipoFrete.length >= 1) boxSelecaoTipoFrete.html(data.labelRestricaoEntrega);
					if(boxSelecaoTipoFrete.length <= 0){
						$('.dadosEntrega .boxRestricaoEntrega').html(data.labelRestricaoEntrega);
					}
				}
				// VERIFICA SE POSSUI CUPOM ATIVO
				verificarCupomAtivo();
			}
			refreshFancybox();
			
			if(data.opcaoFreteMultiEstoque || data.opcoesFreteSimples) {
				dataLayer.push({"event": "shipping_calculation"});
			}
		},
		error: function error(data) {
			//mensagem de erro
			console.log(data);
		}
	});
}

function getPopupEntregaAgendada(sessao, multiEstoque = false, prazo = 0) {
	// REMOVE O LINK ANTIGO
	let elemento = $('.abrirEntregaAgendada');
	elemento.prop('href', '');

	// GERA O NOVO LINK
	let link    = elemento.attr('data-url-action');
	let idFrete = elemento.attr('data-id-frete');
	
	// ADICIONA OS PARÂMETROS
	let params = [
		{ chave: "idFreteAgendar", valor: idFrete },
		{ chave: "prazoMaximo", valor: prazo },
		{ chave: "sessao", valor: sessao },
		{ chave: "multiEstoque", valor: multiEstoque }
	];
	
	let queryString = params.map(param => `${param.chave}=${param.valor}`).join('&');
	link           += '?'+ queryString;

	// ACIONA A POPUP
	elemento.prop('href', link);
	elemento.click();
}

/**
 * Método responsável por limpar os erros de entrega
 * @method limparErroReCaptcha
 */
function limparErroReCaptcha(token){
	$('*[data-callback=limparErroReCaptcha]').parent().find('.formError').remove();
}

/**
 * Função responsável por adicionar as classes do validation engine quando o reCAPTCHA expirar
 */
function adicionarValidacaoReCaptcha() {
	$('*[data-expired-callback=adicionarValidacaoReCaptcha]').addClass('validate[required, custom[recaptcha]]');
}

/******************************************* PAGAMENTO ***************************************/
var formularioPagamentoLiberado = false;
$(document).on('submit', '.formPagamentoSubmit', function (e) {
	e.preventDefault();

	if($(this).find('[name="formulario-pagarme-v5"]').length > 0) return false;

	enviarFormulario(e.target);
});

/**
 * Função responsável por enviar os dados do formulário de pagamento para o ajax-processar-venda
 * @param {Object} formulario O Formulário de pagamento sendo enviado
 */
function enviarFormulario(formulario) {
	//PAGAMENTOS QUE REQUEREM DE VALIDAÇÃO EXTERNA
	var arrayFormValidacaoExterna = ['formPagamentoCartaoCreditoMercadoPago', 'formPagamentoCartaoCreditoPagSeguro', 'formPagamentoBoletoPagSeguro', 'formPagamentoCartaoCreditoPaypalPlus'];
	if (!formularioPagamentoLiberado && jQuery.inArray($(formulario).attr('id'), arrayFormValidacaoExterna) >= 0) return false;

	var preloader = new Preloader();
	preloader.show('Processando pagamento...');

	//ATUALIZA O CAMPO DE SESSION ID DO FORMULÁRIO ATUAL, ATRIBUINDO O SESSION ID PREENCHIDO PELO SCRIPT DO FINGERPRINT DA CLEARSALE
	if($(formulario).find('.sessionIdFingerprint').length) $(formulario).find('.sessionIdFingerprint').val($('#sessionIdFingerprint').val());

	if($(formulario).find('[name="finger_print"]').length) $(formulario).find('.sessionIdFingerprint').val($('[name="finger_print"]').val());

	var formData = new FormData(formulario);
	jQuery.ajax({
		url: CAMINHO + '/checkout/ajax/ajax-processar-venda.php',
		type: 'POST',
		dataType: 'json',
		data: formData,
		cache: false,
		contentType: false,
		processData: false,
		success: function success(data) {
			tratarRetornoProcessarVenda(data, preloader);
		},
		error: function error(e) {
			//mensagem de erro
			console.log(e);
		}
	});
	return false;
}

/**
 * Botao para gerar orçamento
 */
$(document).on('click', '.gerar-orcamento', function (e) {
	var preloader = new Preloader();
	preloader.show('Gerando seu orçamento...');

	jQuery.ajax({
		url: CAMINHO + '/checkout/ajax/ajax-checkout-gerar-orcamento.php',
		type: 'GET',
		dataType: 'json',
		success: function success(resposta) {
			preloader.hide();
			window.scrollTo(0, $('.boxProdutoCarrinho').position().top);

			if(resposta.url.length) {
				window.location = resposta.url;
			}

			if (!resposta.erro) {
				window.open(CAMINHO + '/checkout/ajax/ajax-checkout-baixar-orcamento.php?arquivo=' + resposta.arquivo + "&sessionId=" + resposta.sessionId);
			}

			$('.boxMensagemOrcamento').html(resposta.mensagem);
		},
	});

});

/**
* Verifica os caracteres do numero do cartão do plugin validate
*/
function checkCartao(field, rules, i, options) {
	var n = field.attr('maxlength');
	var relacionado = field.closest('form').find('#' + field.data('id'));

	if (!$(relacionado).val().length) {
		return "Número do cartão inválido. Bandeira não identificada";
	} else if ($(relacionado).val() == CARTAO_MASTER) {
		if (field.val().length < 12 || field.val().length > 19) {
			return "Número do cartão inválido (deve conter entre 12 e 19 dígitos)";
		}
	} else if ($(relacionado).val() == CARTAO_AURA || $(relacionado).val() == CARTAO_HIPERCARD) {
		if (field.val().length < 16 || field.val().length > 19) {
			return "Número do cartão inválido (deve conter entre 16 e 19 dígitos)";
		}
	} else if (field.val().length != n) return "Número do cartão inválido (" + n + " dígitos)";
}

/**
* Verifica a bandeira e os numeros de digitos do código de segurança do plugin validate
*/
function checkCCV(field, rules, i, options){
	n = 3;

  var seletorIdPagamento = field.attr('name') == 'cartao2[codSeguranca]' ? 'idPagamento2' : 'idPagamento';
	var idPagamento        = $('#'+field.data('id')).find('#'+seletorIdPagamento).val();

	if (idPagamento == CARTAO_AMEX) n = 4;


	if (field.val().length != n) return "Código de segurança inválido (" + n + " dígitos)";
}

$(document).ready(function () {
	$('.boxCheckout-codSeg').attr('maxlength', 4);
});


//POPUP DE RECOMENDAÇÃO
$(document).ready(function(){
	if($('#recomendacaoLink').length){
		$("#recomendacaoLink").trigger('click');
    return;
	}

  if($("#selecionarMeiosPagamentoLink").length)
    $("#selecionarMeiosPagamentoLink").trigger('click');
});

/**
 * Ação responsável por fazer a busca de parcelas quando o valor de pagamento no form de cartão de crédito mudar
 */
$(document).on('keyup change', 'div[data-boxpag-item-divisao] form.cartaoCredito .mainBox-conteudo-formDiv-input-saldo', function() {
	const campoCartao = $($(this).closest('form').find('.inputCartao'));

	// CASO O CAMPO NÃO ESTEJA PREENCHIDO NÃO PRECISA RODAR O RESTANTE
	if(!campoCartao.val()) return;

	detectaBandeiraCartao(campoCartao);
});

/**
 * Campo de input do numero do cartao
 */
$(document).on('keyup change', '.inputCartao', function () {
	detectaBandeiraCartao($(this));
});

/**
 * Função que detecta a bandeira utilizada atravez de regex
 */
function detectaBandeiraCartao(campo) {
	tratarNumeroCartao(campo);
	var campoId = '#' + campo.data('id');
	var valor 	= campo.val();

	if (valor && valor.length > 12) {
		reg.forEach(function (entry, key) {
			var patt = new RegExp(reg[key]);
			var res = patt.test(valor);

			if (res) {
				var bandeira = regCard[key];

				$(campo).attr('maxlength', regCardMaxlen[key]);
				$(campo).closest('form').find(campoId).val(key);

				//ATIVA BANDEIRA
				var bandeiras = ($(campo).data('bandeiras')) ? $(campo).closest('form').find('#' + $(campo).data('bandeiras')) : $(campo).closest('form').find('#' + $(campo).data('bandeiras-dois'));
				bandeiras.find('.cartaoUtilizado-cartoes').removeClass('ativo');
				bandeiras.find('.cartaoUtilizado-cartoes.' + bandeira).addClass('ativo');

				if ($(campo).attr('data-parcelas')) {
					let valorPago = $(campo).closest('form').find('.mainBox-conteudo-formDiv-input-saldo').val() ?? '';
					valorPago     = Number(valorPago.replace('.', '').replace(',', '.'));

					buscarParcelasCartao(bandeira, $(campo).attr('data-parcelas'), valorPago);
				} else if ($(campo).data('parcelas-dois')) {
					buscarParcelasMultiMeios('cartao', campo);
				}
				return;
			}
		});
	} else {
		limparBandeiraCartao(campo);
	}
}

/**
 * Função remove os espaços do número do cartão
 */
function tratarNumeroCartao(campo) {
	campo.val(campo.val().replace(/\s/g,''));
}

/**
 * Função responsável por buscar as parcelas para o formulário de cartão
 * @param {String} bandeira  Bandeira do cartão
 * @param {String} id        ID do campo de parcelas
 * @param {Number} valorPago Valor pago específico no caso de mix de pagamento
 */
function buscarParcelasCartao(bandeira, id, valorPago) {
	var campoParcelas = $('#' + id);

	const dados = {
		'bandeira': bandeira,
	};

	if(valorPago > 0) {
		dados.valorPago = valorPago;
	}

	jQuery.ajax({
		type: "POST",
		data: dados,
		dataType: 'JSON',
		url: CAMINHO + "/checkout/ajax/ajax-checkout-parcelas.php",
		success: function success(response) {
			campoParcelas.html(response.parcelas);
			campoParcelas.removeAttr('readonly');

			atualizarRTPorParcelasCartao($(campoParcelas))
		},
		error: function error(response) {
			campoParcelas.html('<option valude="">Parcelas indisponíveis</otpion>');
			campoParcelas.prop('readonly', true);
		}
	});
}

/**
 * Função remove as bandeiras ativas deixando todas desativadas
 */
function limparBandeiraCartao(campo) {
	var formulario = $(campo).closest('form');
	if ($(campo).data('parcelas')) {
		var campoParcelas = $('#' + $(campo).data('parcelas'));
		var bandeiras = formulario.find('#' + $(campo).data('bandeiras'));
	} else if ($(campo).data('parcelas-dois')) {
		var campoParcelas = formulario.find('#' + $(campo).data('parcelas-dois'));
		var bandeiras = formulario.find('#' + $(campo).data('bandeiras-dois'));
	}
	if (bandeiras) {
		var pagamento = formulario.find('#' + $(campo).data('id'));
		pagamento.val('');
		bandeiras.find('.cartaoUtilizado-cartoes').removeClass('ativo');
		campoParcelas.html('<option valude="">Preencha o número do cartão</otpion>');
		campoParcelas.prop('readonly', true);
	}
}

/**
 * Função responsável por buscar as parcelas para dois cartões
 */
function buscarParcelasMultiMeios(tipoAlteracao, campo) {
	var formulario = $(campo).closest('form');
	var boxDadosCartao = $(campo).closest('.boxDadosCartao');
	var pagamento = $(campo).data('id') == 'idPagamento2' ? 2 : 1;
	var valorPagamentoUm = formulario.find('#valorCartao1').val();
	var idPagamentoUm = formulario.find('#idPagamento').val();
	var idPagamentoDois = formulario.find('#idPagamento2').val();

	var parcelasPrimeiroCartao = formulario.find('.parcelasPrimeiroCartao');
	var parcelasSegundoCartao = formulario.find('.parcelasSegundoCartao');

	var tipoPagamento = 'doisCartoes';
	switch (formulario.attr('name')) {
		case 'formPagamentoBoletoCartao':
			tipoPagamento = 'boletoCartao';
			break;
	}

	//CARREGA AS PARCELAS
	jQuery.ajax({
		type: "POST",
		data: { 'valorPagamentoUm': valorPagamentoUm, 'tipoPagamento': tipoPagamento, 'idPagamentoUm': idPagamentoUm, 'idPagamentoDois': idPagamentoDois },
		url: CAMINHO + "/checkout/ajax/ajax-checkout-parcelas-dois-cartoes.php",
		dataType: 'json',
		success: function success(data) {
			if (tipoAlteracao == 'valor' || pagamento == 1) {
				parcelasPrimeiroCartao.removeAttr('readonly');
				parcelasPrimeiroCartao.html(data.parcelasCartaoUm); //PARCELAS DO CARTÃO 1
				if (isNaN(idPagamentoUm) || idPagamentoUm <= 0) {
					parcelasPrimeiroCartao.prop('readonly', true);
				}
			}
			if (tipoAlteracao == 'valor' || pagamento == 2) {
				parcelasSegundoCartao.removeAttr('readonly');
				parcelasSegundoCartao.html(data.parcelasCartaoDois); //PARCELAS DO CARTÃO 2
				if (isNaN(idPagamentoDois) || idPagamentoDois <= 0) {
					parcelasSegundoCartao.prop('readonly', true);
				}
			}
			//VOLTA OS VALORES PARA OS CAMPOS
			formulario.find('#valorCartao1').val(data.valorPagamentoUm.replace('.', ','));
			formulario.find('#valorCartao2').val(data.valorPagamentoDois.replace('.', ','));
			formulario.find('#valorBoleto').html('R$ ' + data.valorPagamentoDois.replace('.', ','));
		},
		error: function error(e) {}
	});
}

/**
* Função calcula as parcelas para o pagamento com dois cartões
*/
$(document).on('change', '#valorCartao1', function (e) {

	var valor1 = parseFloat($(this).val().replace(',', '.')).toFixed(2); //PEGA O VALOR DO CAMPO DO VALOR DO CARTÃO QUE PODE SER DIGITADO
	var valor2 = (PED_TOTAL - valor1).toFixed(2); //VALOR DO SEGUNDO CARTÃO (TOTAL - VALOR1)

	//VALIDAÇÃO DOS VALORES
	//SE O VALOR 2 FOR MENOR QUE O MÍNIMO
	if (valor2 < VALOR_MIN_DOIS_CARTOES) {

		valor2 = VALOR_MIN_DOIS_CARTOES.toFixed(2);
		valor1 = (PED_TOTAL - VALOR_MIN_DOIS_CARTOES).toFixed(2);

		//SE O VALOR 1 FOR MENOR QUE O MÍNIMO
	} else if (valor1 < VALOR_MIN_DOIS_CARTOES) {

		valor1 = VALOR_MIN_DOIS_CARTOES.toFixed(2);
		valor2 = (PED_TOTAL - VALOR_MIN_DOIS_CARTOES).toFixed(2);
	}

	if (isNaN(valor1) || isNaN(valor2)) {
		valor1 = (PED_TOTAL / 2).toFixed(2);
		valor2 = (PED_TOTAL / 2).toFixed(2);
	}

	//VOLTA OS VALORES PARA OS CAMPOS
	$('#valorCartao1').val(valor1.replace('.', ','));
	$('#valorCartao2').val(valor2.replace('.', ','));
	$('#valorBoleto').html('R$ ' + valor2.replace('.', ','));

	buscarParcelasMultiMeios('valor', this);
});

/**
* Método formata valor em moeda
* @param  int number        Valor a formatar
* @param  int decimals      Quantidade de casas finais
* @param  string dec_point   Pontuação decimal
* @param  string thousands_sep pontuação milhar
* @return float Retorna o numero formatado
*/
function number_format(number, decimals, dec_point, thousands_sep) {
	number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
	var n = !isFinite(+number) ? 0 : +number,
	    prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
	    sep = typeof thousands_sep === 'undefined' ? ',' : thousands_sep,
	    dec = typeof dec_point === 'undefined' ? '.' : dec_point,
	    s = '',
	    toFixedFix = function toFixedFix(n, prec) {
		var k = Math.pow(10, prec);
		return '' + Math.round(n * k) / k;
	};
	/* Fix for IE parseFloat(0.55).toFixed(0) = 0; */
	s = (prec ? toFixedFix(n, prec) : '' + Math.round(n)).split('.');
	if (s[0].length > 3) {
		s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
	}
	if ((s[1] || '').length < prec) {
		s[1] = s[1] || '';
		s[1] += new Array(prec - s[1].length + 1).join('0');
	}
	return s.join(dec);
}

/**
 * Função responsável por formatar o preço na moeda atual
 * @param   {Number}  number              Número a ser formatado
 * @param   {Boolean} incluirSimboloMoeda Determina se será colocado o símbolo da moeda
 * @returns {String}                      Valor formatado na moeda
 */
function formatarPreco(number, incluirSimboloMoeda = true) {
	const currency = (typeof WAPSTORE_CODIGO_MOEDA !== 'undefined') ? WAPSTORE_CODIGO_MOEDA : 'BRL';
	const regiao   = (typeof WAPSTORE_CODIGO_LINGUAGEM !== 'undefined') ? WAPSTORE_CODIGO_LINGUAGEM : 'pt-BR';

	number = Number(number);

	return number.toLocaleString(
		regiao,
		{
			currency,
			style: incluirSimboloMoeda == true ? 'currency' : 'decimal',
			maximumFractionDigits: 2
		}
	);
}

/**
 * Função responsável por converter um valor em padrão americano
 * @param   {String} valor Valor para formatar
 * @returns {Number}       Valor em padrão americano
 */
function formatarFloat(valor) {
	const currency = (typeof WAPSTORE_CODIGO_MOEDA !== 'undefined') ? WAPSTORE_CODIGO_MOEDA : 'BRL';

	switch(currency) {
		case 'BRL':
			valor = valor.replaceAll('.', '').replace(',', '.');
			break;

		case 'USD':
			valor = valor.replaceAll(',', '');
			break;
	}

	// REMOVE OS PREFIXOS DE MOEDA
	return Number(valor.replace(/[^\d.]+/, ''));
}

/**
* Ativar o validation
*/
refreshValidation();
function refreshValidation() {
	$('.validacao').validationEngine('attach', {
		showOneMessage: true,
		promptPosition: 'bottomLeft'
	});
}

